         MACRO                                                          03500000
&NAME    RACXTRT &ACEE=,                                               *03550000
               &ENCRYPT=,                                              *03600000
               &ENTITY=,                                               *03650000
               &ENTITYX=,                                          @M2A*03700000
               &FIELDS=,                                               *03750000
               &SUBPOOL=,                                              *03800000
               &TYPE=,                                                 *03850000
               &RELEASE=,                                              *03900000
               &CLASS=,                                            @L8A*03950000
               &SEGMENT=,                                          @L8A*04000000
               &SEGDATA=,                                          @L8A*04050000
               &VOLSER=,                                           @L8A*04100000
               &GENERIC=,                                          @L8A*04150000
               &DERIVE=,                                           @P1A*04200000
               &FLDACC=,                                           @L8A*04250000
               &BRANCH=,                                           @M1A*04300000
               &MATCHGN=,                                          @M1A*04350000
               &MF=S                                                    04400000
.*  DEFINE LOCAL VARIABLES                                              04450000
         LCLA  &FN                 USED TO CONTAIN NUMERIC VALUE        04500000
.*                                 FOR FUNCTION                         04550000
         LCLA  &VERS               USED TO CONTAIN VERSION NUMBER  @L8A 04600000
.*                                 TESTS THAT LIST AND EXECUTE FORMS    04650000
.*                                 HAVE SAME RELEASE NUMBER.            04700000
         LCLA  &ENC                USED TO CONTAIN ADDRESS OF DATA      04750000
.*                                 TO BE ENCRYPTED                      04800000
         LCLA  &DFLTSP             USED TO SET DEFAULT SUBPOOL          04850000
.*                                 WHEN ENTERED AS A STRING             04900000
         LCLC  &SEG000             SEGMENT NAME (ADDRS OR CONST)   @L8A 04950000
         LCLC  &CLS000             SEGMENT NAME (ADDRS OR CONST)   @L8A 05000000
         LCLA  &GENFLG             GENERIC PROCESSING FLAG         @L8A 05050000
         LCLA  &DRVFLG             DFP PROCESSING FLAG             @L8A 05100000
         LCLA  &FLDACF             FLDACC PROCESSING FLAG          @L8A 05150000
         LCLA  &BIT0,&BIT1,&BIT2,&BIT3,&BIT4,&BIT5,&BIT6,&BIT7     @L8A 05200000
         LCLA  &BITA0,&BITA1,&BITA2,&BITA3                              05250000
         LCLA  &BITA4,&BITA5,&BITA6,&BITA7                              05300000
         LCLB  &RLVLCHK            USED TO CONTROL RELEASE-CHECK   @L7A 05350000
         LCLC  &METH               USED TO CONTAIN ENCRYPTION METHOD    05400000
         LCLC  &SP                 USED TO CONTAIN SUBPOOL VALUE        05450000
         LCLC  &NX                 USED TO CONTAIN SYSNDX FOR LABELS    05500000
         LCLB  &YCOPENT            0 => ENTITY IS SPECIFIED        @M2A 05550000
.*                                 1 => ENTITYX IS SPECIFIED       @M2A 05600000
         LCLC  &CMENTY             COMMON ENTITY VARIABLE          @M2A 05650000
.*                                                                 @M2A 05700000
.*  DEFINE GLOBAL VARIABLES                                             05750000
         GBLC  &ICHL               USED FOR ADDRESS OF EXTRACT PARM     05800000
         GBLA  &ICHCT              ICHCT - A COUNTER USED TO KEEP       05850000
.*                                 TRACK OF WHERE IN THE PARAMETER      05900000
.*                                 LIST THE PARAMETER IS TO BE          05950000
.*                                 PLACED. ICHCT IS INITIALIZED TO      06000000
.*                                 THE FIRST FULLWORD IN THE LIST       06050000
.*                                 TO BE FILLED IN BY ICHMM,            06100000
.*                                 AND INCREMENTED BY THE INNER         06150000
.*                                 MACRO (ICHMM).                       06200000
         GBLB  &ICHRAC             ICHRAC - A FLAG WHICH IS             06250000
.*                                 INITIALIZED IN THE RACROUTE MACRO    06300000
.*                                 TO COMMUNICATE TO RACXTRT THAT IT    06350000
.*                                 IS BEING INVOKED AS AN INNER MACRO   06400000
.*                                 TO THE RACROUTE MACRO.               06450000
         GBLB  &ICHLIST            ICHLIST - A FLAG WHICH IS SET TO     06500000
.*                                 INDICATE THE FORMAT OF THE MACRO.    06550000
.*                                 FOR LIST FORMAT, ICHLIST IS ON       06600000
.*                                 FOR EXECUTE FORMAT, ICHLIST IS       06650000
.*                                 OFF. FOR STANDARD FORMAT, THERE      06700000
.*                                 ARE TWO PASSES DONE BY THE MACRO     06750000
.*                                 A LIST PASS AND AN EXECUTE PASS.     06800000
.*                                 INITIALLY, FOR STANDARD, THE         06850000
.*                                 FLAG  IS ON - FOR THE LIST PASS.     06900000
.*                                 IT IS THEN TURNED OFF FOR THE        06950000
.*                                 EXECUTE PASS.                        07000000
         GBLB  &ICHNTFY            ICHNTFY - A GLOBAL INDICATOR         07050000
.*                                 WHICH IS USED TO COMMUNICATE WITH    07100000
.*                                 ICHMM - THE INNER MACRO. IF ICHNTFY  07150000
.*                                 IS ON, THE INNER MACRO WILL NOT      07200000
.*                                 ISSUE ITS OWN MNOTE IF IT FINDS AN   07250000
.*                                 ERROR-IT WILL MERELY TURN ON THE     07300000
.*                                 ICHINER FLAG AS NOTIFICATION TO THE  07350000
.*                                 OUTER MACRO, WHICH WILL TAKE THE     07400000
.*                                 APPROPRIATE ACTION.                  07450000
.*                                 IF ICHNTFY IS OFF, THE INNER MACRO   07500000
.*                                 WILL TAKE ACTION ON ITS OWN,         07550000
.*                                 IE. ISSUE AN MNOTE WITH A SEVERITY   07600000
.*                                 CODE OF ITS CHOOSING.                07650000
         GBLB  &ICHINER            ICHINER - A GLOBAL INDICATOR         07700000
.*                                 WHICH IS USED BY ICHMM - AN INNER    07750000
.*                                 MACRO - TO TELL THE OUTER MACRO IF   07800000
.*                                 AN ERROR HAS BEEN DETECTED IN A      07850000
.*                                 PARAMETER.  IF ICHINER IS ON, AN     07900000
.*                                 ERROR HAS BEEN DETECTED.             07950000
         GBLC  &ICHMF              ICHMF - AN INDICATOR OF THE          08000000
.*                                 FORMAT - L, E, S OR M           @M1C 08050000
         GBLC  &ICHRLVL            SYMBOL TO BRANCH TO             @L7A 08100000
.*                                 WHEN INCOMPATIBLE MACRO LEVEL.  @L7A 08150000
.* VALIDATE RELEASE PARAMETER                                      @L7A 08200000
         AIF   (&ICHRAC).RLVL01                                    @L7A 08250000
         AGO   .ERRVSE             NOT INVOKED BY RACROUTE     @D62CDHH 08275000
&ICHRLVL SETC  'ICH&SYSNDX.X'      CREATE LABEL FOR SVC BYPASS     @L7A 08300000
.RLVL01  ANOP                                                      @L7A 08350000
&VERS    SETA  0                   DEFAULT VERSION NUMBER          @L8A 08400000
&YCOPENT SETB  0                   ASSUME THAT THE ENTITY IS       @M2A 08450000
&CMENTY  SETC  '&ENTITY'           SPECIFIED.                      @M2A 08500000
.*                                                                 @M2A 08550000
.* VALIDATE THE RELEASE PARAMETER                                  @M2A 08600000
.*                                                                 @M2A 08650000
         AIF   (N'&RELEASE GT 2).ERLVL1  INVALID RELEASE PARAMETER @L7A 08700000
         AIF   ('&RELEASE(1)' EQ '').VLIDREL                       @M2A 08750000
         AIF   ('&RELEASE(1)' EQ '1.6').VLIDREL                    @M2A 08800000
         AIF   ('&RELEASE(1)' EQ '1.7').VLIDREL                    @M2A 08850000
         AIF   ('&RELEASE(1)' EQ '1.8').VLIDREL                    @M2A 08900000
         AIF   ('&RELEASE(1)' EQ '1.8.1').VLIDREL                  @M2A 08950000
         AIF   ('&RELEASE(1)' EQ '1.9').VLIDREL                    @M2A 09000000
         AIF   ('&RELEASE(1)' EQ '1.9.2').VLIDREL                  @M3A 09050000
         AIF   ('&RELEASE(1)' EQ '2.1').VLIDREL                    @M4A 09066600
         AIF   ('&RELEASE(1)' EQ '2.2').VLIDREL                    @M5A 09083200
         AGO   .ERLVL1                                             @M2A 09100000
.VLIDREL ANOP                                                      @M2A 09150000
.*                                                                 @M2A 09200000
.* IF SECOND OPERAND IS SPECIFIED TEST THAT IT IS "CHECK" AND THE  @M2A 09250000
.* THE MACRO FORMAT IS 'E'                                         @M2A 09300000
.*                                                                 @M2A 09350000
         AIF   (N'&RELEASE LE 1).NORLVLC     CHECK NOT SPECIFIED   @L7A 09400000
         AIF   ('&RELEASE(2)' NE 'CHECK').ERLVL1   INVALID VALUE   @L7A 09450000
         AIF   ('&MF(1)' NE 'E').ERLVL2     CHECK ONLY WITH MF=E   @L7A 09500000
&RLVLCHK SETB  1                          ACTIVATE RELEASE-CHECK   @L7A 09550000
.ERLVL2R ANOP                                                      @L7A 09600000
.NORLVLC ANOP                                                      @L7A 09650000
.*                                                                 @M2A 09700000
.* TEST INCOMPATABILITY OF NEW KEYWORDS.                           @M2A 09750000
.*                                                                 @M2A 09800000
         AIF   ('&RELEASE(1)' EQ '').RLCHK16                       @P5A 09850000
         AIF   ('&RELEASE(1)'(1,3) GE '1.9').RLCHK19               @P5C 09900000
         AIF   ('&RELEASE(1)' GE '1.8.1').RLCHK18                  @M2A 09950000
         AIF   ('&RELEASE(1)' GE '1.8').RLCHK18                    @M2A 10000000
.*                                                                 @M2A 10050000
.* KEYWORDS INTRODUCED IN RELEASE 1.8 ARE NOT SPECIFIED WITH       @M2A 10100000
.* PREVIOUS RELEASES.                                              @M2A 10150000
.*                                                                 @M2A 10200000
.RLCHK16 ANOP                                                      @P5A 10250000
         AIF   ('&SEGMENT&SEGDATA&ACEE&VOLSER&GENERIC&FLDACC&CLASS&DERIX10300000
               VE' NE  '').ERR14                RELEASE IS 1.6/1.7 @M2C 10350000
.ERTN14  ANOP                                                      @M2C 10400000
         AIF   ('&TYPE' NE 'EXTRACT' AND '&TYPE' NE '' AND             X10450000
               '&TYPE' NE 'ENCRYPT').ERR15                         @M2C 10500000
.ERTN15  ANOP                                                      @M2C 10550000
.*                                                                 @M2A 10600000
.* KEYWORDS INTRODUCED IN RELEASE 1.9 ARE NOT SPECIFIED WITH       @M2A 10650000
.* PREVIOUS RELEASES.                                              @M2A 10700000
.*                                                                 @M2A 10750000
.RLCHK18 ANOP                                                      @M2A 10800000
         AIF   ('&BRANCH&MATCHGN' NE '').ERR14X                    @M2C 10850000
.ERTN14X ANOP                                                      @M2A 10900000
         AIF   ('&ENTITYX' NE '').ERR26                            @M2A 10950000
.ERTN26  ANOP                                                      @M2A 11000000
.RLCHK19 ANOP                                                      @M2A 11050000
.*                                                                 @M2A 11100000
.* UPWARD COMPATBILITY OF THE CODE.                                @M2A 11150000
.*                                                                 @M2A 11200000
         AIF  ('&RELEASE' LE '1.7').RL17COD                        @M2A 11250000
         AIF  ('&RELEASE' EQ '1.8').RL18COD                        @M2A 11300000
         AIF  ('&RELEASE' EQ '1.8.1').RL18COD                      @M2A 11350000
         AIF   (NOT &ICHRAC).ERR23       NOT CALLED BY RACROUTE    @M2A 11400000
.ERTN23  ANOP                            ISSUE MNOTE.              @M2A 11450000
.************************************************************      @M1A 11500000
.* VALIDITY CHECK BRANCH KEYWORD                                   @M1A 11550000
.************************************************************      @M1A 11600000
         AIF   ('&BRANCH' EQ 'NO' OR '&BRANCH' EQ '').BRCH010      @M1A 11650000
         AIF   ('&BRANCH' NE 'YES').ERR24                          @M1A 11700000
         AIF   ('&TYPE' NE 'EXTRACT' AND                               X11750000
               '&TYPE' NE 'ENCRYPT').ERR22                         @M1A 11800000
.ERTN22  ANOP                                                      @M1A 11850000
.BRCH010 ANOP                                                      @M1A 11900000
.*                                                                 @M2A 11950000
.* TEST 1. ENTITY AND ENTITYX ARE MUTUALLY EXCLUSIVE.              @M2A 12000000
.*      2. SET FLAG "YCOPENTY" WHEN ENTITYX IS SPECIFIED.          @M2A 12050000
.*                                                                 @M2A 12100000
         AIF   ('&ENTITY' NE '' AND '&ENTITYX' NE '' ).ERR25       @M2A 12150000
.ERTN25  ANOP                                                      @M2A 12200000
         AIF   ('&ENTITYX' EQ '').NOCOPY                           @M2A 12250000
&YCOPENT SETB  1                  SET FLAG AS ENTITYX IS SPECIFIED @M2A 12300000
&CMENTY  SETC  '&ENTITYX'                                          @M2A 12350000
.NOCOPY  ANOP                                                      @M2A 12400000
.RL18COD ANOP                                                      @M2A 12450000
&VERS    SETA  1                                                   @M2A 12500000
.RL17COD ANOP                                                      @M2A 12550000
         AIF   ('&TYPE' NE 'ENCRYPT' AND T'&ENCRYPT NE 'O').ERR16  @M1C 12600000
.ERLVL1R ANOP                                                      @L7A 12650000
.************************                                               12700000
.*   INITIALIZE VARIABLES                                               12750000
.************************                                               12800000
.INITVAR ANOP                                                      @L8A 12850000
&NX      SETC  '&SYSNDX'                                                12900000
&DFLTSP  SETA  229                 DEFAULT SUBPOOL                      12950000
&ICHNTFY SETB  0                   WANT ICHMM TO ISSUE ERROR MSGS       13000000
&ICHINER SETB  0                   INITIALIZE ICHINER TO 0              13050000
.***********************************                                    13100000
.*   SET UP INDICATORS FOR MF FORMAT                                    13150000
.***********************************                                    13200000
.*  VALIDATE MF PARAMETER                                               13250000
         AIF   ('&MF' NE 'L' AND '&MF' NE 'S' AND '&MF(1)' NE          *13300000
               'E' AND '&MF(1)' NE 'M').ERR01                      @M1C 13350000
&ICHMF   SETC  '&MF(1)'        SET MF TYPE                              13400000
         AIF   ('&MF(1)' NE 'M').MFLSE                             @M1A 13450000
         AIF   (NOT &ICHRAC).ERR01  MF=M ONLY ALLOWED VIA RACROUTE @M1A 13500000
&ICHMF   SETC  'E'             TREAT MF=M SAME AS MF=E             @M1A 13550000
.MFLSE   ANOP                           MF=L, S OR E               @M1A 13600000
         AIF   ('&ICHMF' EQ 'E').EXT0030                           @M1C 13650000
&ICHLIST SETB  1               MF LIST OR STANDARD                      13700000
         AIF   (T'&SEGMENT EQ 'O').SEG000                          @L8A 13750000
         AIF   ('&SEGMENT'(1,1) EQ '''').SEGL000                   @L8A 13800000
.SEG000  ANOP                                                      @L8A 13850000
&SEG000  SETC  '&SEGMENT'                                          @L8A 13900000
         AGO   .SEGL999                                            @L8A 13950000
.SEGL000 ANOP  ,                                                   @L8A 14000000
&SEG000  SETC  'ICH&NX.S'                                          @L8A 14050000
.SEGL999 ANOP  ,                                                   @L8A 14100000
         AIF   (T'&CLASS EQ 'O').CLS000                            @L8A 14150000
         AIF   ('&CLASS'(1,1) EQ '''').CLSL000                     @L8A 14200000
.CLS000  ANOP                                                      @L8A 14250000
&CLS000  SETC  '&CLASS'                                            @L8A 14300000
         AGO   .CLSL999                                            @L8A 14350000
.CLSL000 ANOP  ,                                                   @L8A 14400000
&CLS000  SETC  'ICH&NX.L'                                               14450000
.CLSL999 ANOP  ,                                                        14500000
         AGO   .CONT01                                                  14550000
.EXT0030 ANOP                                                           14600000
         AIF   ('&MF(2)' EQ '' OR N'&MF NE 2).ERR01                     14650000
         AIF   (T'&SEGMENT EQ 'O').SEG001                          @L8A 14700000
         AIF   ('&SEGMENT'(1,1) EQ '''').ERR17                     @L8A 14750000
&SEG000  SETC  '&SEGMENT'                                          @L8A 14800000
.ERTN17  ANOP  ,                                                   @L8A 14850000
.SEG001  AIF   (T'&CLASS EQ 'O').CLS001                            @L8A 14900000
         AIF   ('&CLASS'(1,1) EQ '''').ERR18                       @L8A 14950000
&CLS000  SETC  '&CLASS'                                            @L8A 15000000
.CLS001  ANOP  ,                                                   @L8A 15050000
.ERTN18  ANOP  ,                                                   @L8A 15100000
&ICHLIST SETB  0               MF EXECUTE                               15150000
.****************************************************                   15200000
.*  DETERMINE FUNCTION SUPPLIED                                         15250000
.****************************************************                   15300000
.CONT01  ANOP                                                           15350000
.TYPECK  ANOP                                                      @L8A 15400000
         AIF   ('&TYPE' EQ '').ERR07                               @L8A 15450000
         AIF   ('&TYPE' EQ 'ENCRYPT').EXT0300                      @L8A 15500000
         AIF   ('&TYPE' EQ 'REPLACE').REP0100                      @L8A 15550000
         AIF   ('&TYPE' EQ 'EXTRACTN').EXT0200                     @L8A 15600000
         AIF   ('&TYPE' NE 'EXTRACT').ERR05                        @L8A 15650000
.****************************************************                   15700000
.*   VALIDATE PARAMETERS ALLOWABLE WITH                                 15750000
.*   TYPE=EXTRACT  TYPE=EXTRACTN TYPE=REPLACE                      @L8A 15800000
.****************************************************                   15850000
.EXT0100 ANOP                                                      @L8A 15900000
&FN      SETA  1                         TYPE=EXTRACT              @L8A 15950000
         AIF   ('&ICHMF' NE 'S').EXCONT1                           @L8A 16000000
         AIF   ('&SEGDATA' NE '').ERR03                            @L8A 16050000
         AGO   .EXCONT1                                            @L8A 16100000
.EXT0200 ANOP                                                      @L8A 16150000
&FN      SETA  3                         TYPE=EXTRACTN             @L8A 16200000
         AIF   ('&ICHMF' NE 'S').EXCONT1                           @L8A 16250000
         AIF   ('&SEGDATA' NE '').ERR03                            @L8A 16300000
         AGO   .EXCONT1                                            @L8A 16350000
.REP0100 ANOP                                                      @L8A 16400000
&FN      SETA  4                         TYPE=REPLACE              @L8A 16450000
         AIF   ('&MF' NE 'S').EXCONT1                              @L8A 16500000
         AIF   ('&SEGMENT&SEGDATA' EQ '').ERR19                    @L8A 16550000
         AIF   ('&SEGDATA' NE '' AND '&FIELDS' EQ '').ERR27        @P4A 16600000
.ERTN27  ANOP                                                      @P4A 16650000
.EXCONT1 ANOP                                                      @L8A 16700000
         AIF   ('&ENCRYPT' NE '').ERR03                            @L8A 16750000
         AIF   ('&SUBPOOL' EQ '').CONT01A                          @L8A 16800000
         AIF   (T'&SUBPOOL NE 'N').ERR09  SUBPOOL NON-NUMERIC      @L8A 16850000
         AIF   (&SUBPOOL LT 0 OR &SUBPOOL GT 255).ERR09            @L8A 16900000
&SP      SETC  '&SUBPOOL'       COPY SUBPOOL VALUE TO LOCAL AREA   @L8A 16950000
         AGO   .CONTSP                                             @L8A 17000000
.CONT01A ANOP  ,                                                        17050000
         AIF   ('&ICHMF' EQ 'E').CONTSP                            @L8A 17100000
&SP      SETC  '&DFLTSP'      SET DEFAULT SUBPOOL IN LOCAL AREA    @L8A 17150000
.CONTSP  ANOP  ,                                                        17200000
         AIF   ('&GENERIC' EQ '' ).DFP                                  17250000
         AIF   ('&GENERIC' NE 'ASIS' AND                               X17300000
               '&GENERIC' NE 'YES').ERR11                          @L8A 17350000
.DFP     ANOP  ,                                                   @L8A 17400000
         AIF   ('&DERIVE' EQ '' ).FLDACC                           @P1A 17450000
         AIF   ('&DERIVE' NE 'YES' AND                                 X17500000
               '&DERIVE' NE 'NO').ERR12                            @P1A 17550000
.FLDACC  ANOP  ,                                                   @L8A 17600000
         AIF   ('&FLDACC' EQ '' ).MATCHG                           @M1C 17650000
         AIF   ('&FLDACC' NE 'YES' AND                                 X17700000
