         LR    0,15                 .TRANSFER TO PARM REG 0             08700000
         AGO   .SF90                    BR TO COMMON CODE               08750000
.*                                      REGISTER FORMS                  08800000
.SF40    SLL   &AXV,16              .ASSURE NO GARBAGE IN HIGH @ZMB0091 08850000
         SRL   &AXV,16              .ORDER BYTES OF AX REG     @ZMB0091 08900000
         AIF   (&AX0 EQ 1).SF50         BR IF SPECIAL REG 0 NOTATION    08950000
         OR    15,&AXV              .OR IN AX VALUE FROM REG            09000000
         LR    0,15                 .TRANSFER TO PARM REG 0             09050000
         AGO   .SF90                    BR TO COMMON CODE               09100000
.SF50    ANOP                           SPECIAL REG 0                   09150000
         OR    0,15                 .OR OPTIONS AND AX VALUE TOGETHER   09200000
.SF90    ANOP                           COMMON INTERFACE CODE           09250000
         SLR   1,1                  .PARM REG 1 MUST BE ZERO            09300000
         ESAR  2                    .EXTRACT SASID                      09350000
         ST    2,16(,13)            .SAVE SASID IN REG 15 SLOT          09400000
         L     2,16(0,0)            .GET ADDR OF CVT                    09450000
         L     2,772(,2)            .GET ADDR OF SYSTEM FUNCTION TABLE  09500000
         L     2,40(,2)             .GET LX/EX FOR ATSET                09550000
         PC    0(2)                 .PC TO ATSET SERVICE                09600000
         L     2,16(,13)            .EXTRACT SAVED SASID                09650000
         SSAR  2                    .RESTORE SASID                      09700000
         LM    2,12,28(13)          .RESTORE REGS 2-12                  09750000
         L     14,12(,13)           .RESTORE REG 14                     09800000
         MEXIT                                                          09850000
