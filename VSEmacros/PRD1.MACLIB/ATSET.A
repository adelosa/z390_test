* IVOPTS     = 0;            /* INDICATE NO OPTIONS SPECIFIED.       */ 12200000
* IVRC       = 0;            /* SET CODE TO GENERATE TEXT            */ 12250000
* IF IVLP1=1 THEN            /* IF POINTER NOTATION          @ZMB0031*/ 12300000
*   DO;                      /* POINTER NOTATION.            @ZMB0031*/ 12350000
*     IVRP1=LENGTH(AX(1));   /* GET LAST CHAR OF SUBLIST     @ZMB0031*/ 12400000
*     IF AX(1,IVRP1:IVRP1)¬=')' THEN /* IF NOT A RIGHT PAREN @ZMB0031*/ 12450000
*       IVRP1=IVLP1;         /* FORCE 'NOT-SPECIFIED' ERROR  @ZMB0031*/ 12500000
*   END;                     /* POINTER NOTATION.            @ZMB0031*/ 12550000
* ELSE                       /* NOT POINTER NOTATION.        @ZMB0031*/ 12600000
*   DO;                      /* NOT POINTER NOTATION.        @ZMB0031*/ 12650000
*     IVLP1=0;               /* NOT POINTER NOTATION.        @ZMB0031*/ 12700000
