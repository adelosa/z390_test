*/********************************************************************/ 09550000
* IF AXLIST ¬= ''            /* AXLIST SPECIFIED?                    */ 09600000
*   THEN DO;                 /* YES - ADD KEYWD TO COMMENT           */ 09650000
*     IVDOCMT = IVDOCMT||' AXLIST'||AXLIST;                             09700000
*     IF AXLIST(1) ¬= '' &   /* WAS ARGUMENT SPECIFIED?      @ZM50111*/ 09750000
