*     ANS MSG('AHA0013 AXLIST IS REQUIRED '||                           10700000
*             'BUT IS NOT SPECIFIED. NO TEXT IS GENERATED.');           10750000
*     IVRC = 12;             /* KILL TEXT GENERATION                 */ 10800000
*   END;                     /* AXLIST NOT SPECIFIED                 */ 10850000
