.********************************************************************** 08300000
*              MACDATE = 80.122                                         08350000
.********************************************************************** 08400000
.*       CHECK FOR EXTRANEOUS POSITIONAL PARMS                        * 08450000
.********************************************************************** 08500000
         AIF   (N'&SYSLIST NE 0).ERR027                                 08550000
.********************************************************************** 08600000
.*       VALIDATE TKLIST PARAMETER IF PRESENT                         * 08650000
.********************************************************************** 08700000
&S(9)    SETB  (T'&TKLIST NE 'O')       IF OPTION SPECIFIED SET BIT     08750000
         AIF   (T'&TKLIST EQ 'O').OK20 IF NOT, GO CHECK LXLIST PARM     08800000
&OPERAND SETC  'TKLIST'                 SET FOR POSSIBLE ERROR MSGS     08850000
&OPVAL   SETC  '&TKLIST'                SET FOR POSSIBLE ERROR MSGS     08900000
.*   DETERMINE WHETHER TKLIST ADDRESS IS RX OR REG TYPE                 08950000
         AIF   ('&TKLIST'(1,1) EQ '(').OK11   BRANCH IF REG TYPE        09000000
&TKLV    SETC  '&TKLIST'                SET VALUE FOR USE IN LOAD INST. 09050000
&LT1     SETC  'A'                      SET TO 'LA' FOR RX TYPE ADDR    09100000
         AGO   .OK20                    GO CHECK LXLIST PARAMETER       09150000
.OK11    ANOP                                                           09200000
&LT1     SETC  'R'                      SET TO 'LR' FOR REG TYPE ADDR   09250000
&TKLV    SETC  '&TKLIST(1)'             EXTRACT REGISTER NAME           09300000
         AIF   (N'&TKLIST NE 1).ERR002  BR IF INVALID REG FORM          09350000
.********************************************************************** 09400000
.*       VALIDATE LXLIST PARAMETER IF PRESENT                         * 09450000
.********************************************************************** 09500000
.OK20    ANOP                                                           09550000
&S(10)   SETB  (T'&LXLIST NE 'O')       IF OPTION SPECIFIED SET BIT     09600000
         AIF   (T'&LXLIST EQ 'O').OK99  IF NOT, GO TO NEXT SEGMENT      09650000
&OPERAND SETC  'LXLIST'                 SET FOR POSSIBLE ERROR MSGS     09700000
&OPVAL   SETC  '&LXLIST'                SET FOR POSSIBLE ERROR MSGS     09750000
.*   DETERMINE WHETHER LXLIST ADDRESS IS RX OR REG TYPE                 09800000
         AIF   ('&LXLIST'(1,1) EQ '(').OK21   BRANCH IF REG TYPE        09850000
&LXLV    SETC  '&LXLIST'                SET VALUE FOR USE IN LOAD INST. 09900000
&LT3     SETC  'A'                      SET TO 'LA' FOR RX TYPE ADDR    09950000
         AGO   .OK99                    GO TO NEXT SEGMENT              10000000
.OK21    ANOP                                                           10050000
&LT3     SETC  'R'                      SET TO 'LR' FOR REG TYPE ADDR   10100000
&LXLV    SETC  '&LXLIST(1)'             EXTRACT REGISTER NAME           10150000
         AIF   (N'&LXLIST NE 1).ERR002  BR IF INVALID REG FORM          10200000
.********************************************************************** 10250000
.* MAP OPTIONS BITS INTO OPTIONS BYTE                                   10300000
.********************************************************************** 10350000
.OK99    ANOP                                                           10400000
.*                                      CURRENTLY NO OPTIONS DEFINED    10450000
&BOPTS   SETC  '&B(1)&B(2)&B(3)&B(4)&B(5)&B(6)&B(7)&B(8)'               10500000
&OPTNI   SETA  128*&N(1)+64*&N(2)+32*&N(3)+16*&N(4)+8*&N(5)+4*&N(6)+2*&X10550000
               N(7)+&N(8)               OPTIONS NI MASK VALUE           10600000
&OPTOI   SETA  128*&O(1)+64*&O(2)+32*&O(3)+16*&O(4)+8*&O(5)+4*&O(6)+2*&X10650000
               O(7)+&O(8)               OPTIONS OI MASK VALUE           10700000
.********************************************************************** 10750000
.*       DETERMINE TYPE OF REQUEST                                    * 10800000
.********************************************************************** 10850000
         AIF   (T'&MF NE 'O').CKMF2     MF OMITTED = STANDARD FORM      10900000
.********************************************************************** 10950000
.*       STANDARD FORM SPECIFIED                                      * 11000000
.********************************************************************** 11050000
&OPERAND SETC  'TKLIST'                                                 11100000
         AIF   ('&TKLV' EQ '').ERR001   REQUIRED OPERAND NOT SPECIFIED  11150000
&OPERAND SETC  'LXLIST'                                                 11200000
         AIF   ('&LXLV' EQ '').ERR001   REQUIRED OPERAND NOT SPECIFIED  11250000
.* GENERATE STANDARD FORM                                               11300000
&GNAME1  SETC  'IHB'.'&SYSNDX'.'1'      TKLIST                          11350000
&GNAME2  SETC  'IHB'.'&SYSNDX'.'2'      LXLIST                          11400000
&GNAMEL  SETC  'IHB'.'&SYSNDX'.'L'      BRANCH LABEL                    11450000
&LABL    DC    0H'0'                .ETCON XM SERVICE PC                11500000
         STM   14,12,12(13)         .SAVE CALLER'S REGS                 11550000
         AIF   ('&LT1' EQ 'R').SF10     BR IF TKLIST WAS REG FORM       11600000
         LA    2,&TKLV              .GET ADDR OF TKLIST                 11650000
         ST    2,&GNAME1            .STORE TKLIST ADDR IN PARM LIST     11700000
         AGO   .SF12                                                    11750000
.SF10    ST    &TKLV,&GNAME1        .STORE TKLIST ADDR IN PARM LIST     11800000
.*                                                                      11850000
.SF12    AIF   ('&LT3' EQ 'R').SF14     BR IF LXLIST WAS REG FORM       11900000
         LA    2,&LXLV              .GET ADDR OF LXLIST                 11950000
         ST    2,&GNAME2            .STORE LXLIST ADDR IN PARM LIST     12000000
         AGO   .SF16                                                    12050000
.SF14    ST    &LXLV,&GNAME2        .STORE LXLIST ADDR IN PARM LIST     12100000
.*                                                                      12150000
.SF16    CNOP  0,4                  .FORCE PARM LIST TO WORD BOUNDARY   12200000
         LA    1,*+8                .POINT REG1 TO PARM LIST            12250000
         B     &GNAMEL              .BRANCH AROUND PARM LIST            12300000
         DC    XL1'00'              .FORMAT BYTE                        12350000
         DC    BL1'&BOPTS'          .OPTIONS BYTE                       12400000
         DC    AL2(0)               .RESERVED                           12450000
&GNAME1  DC    A(0)                 .TKLIST ADDRESS                     12500000
&GNAME2  DC    A(0)                 .LXLIST ADDRESS                     12550000
&GNAMEL  ESAR  2                    .EXTRACT SASID                      12600000
         ST    2,16(,13)            .SAVE SASID IN REG 15 SLOT          12650000
         L     2,16(0,0)            .GET ADDR OF CVT                    12700000
         L     2,772(,2)            .GET ADDR OF SYSTEM FUNCTION TABLE  12750000
         L     2,16(,2)             .GET LX/EX FOR ETCON                12800000
         PC    0(2)                 .PC TO ETCON SERVICE                12850000
         L     2,16(,13)            .EXTRACT SAVED SASID                12900000
         SSAR  2                    .RESTORE SASID                      12950000
         LM    2,12,28(13)          .RESTORE REGS 2-12                  13000000
         L     14,12(,13)           .RESTORE REG 14                     13050000
         MEXIT                                                          13100000
.********************************************************************** 13150000
.CKMF2   ANOP                                                           13200000
&OPERAND SETC  'MF'                     SET FOR POSSIBLE ERROR MSGS     13250000
&OPVAL   SETC  '&MF'                                                    13300000
         AIF   ('&MF' NE 'L').CKMF3     IF NOT LIST FORM CK FOR EXECUTE 13350000
.********************************************************************** 13400000
.*       LIST FORM SPECIFIED                                          * 13450000
.********************************************************************** 13500000
&OPERAND SETC  'REG FORM TKLIST ADDRESS'                                13550000
&OPVAL   SETC  'MF=L'                                                   13600000
         AIF   ('&LT1' EQ 'R').ERR280   &OPERAND INVALID WITH &OPVAL    13650000
&OPERAND SETC  'REG FORM LXLIST ADDRESS'                                13700000
         AIF   ('&LT3' EQ 'R').ERR280   &OPERAND INVALID WITH &OPVAL    13750000
&LABL    DS    0F'0'                .ETCON PC PARAMETER LIST            13800000
         DC    XL1'00'              .FORMAT BYTE                        13850000
         DC    BL1'&BOPTS'          .OPTIONS FLAG BYTE                  13900000
         DC    AL2(0)               .RESERVED                           13950000
.*                                                                      14000000
         AIF   ('&TKLV' NE '').LF10     BR IF TKLIST SPECIFIED          14050000
&TKLV    SETC  '0'                      MUST BE ZERO IF NOT SPECIFIED   14100000
.LF10    AIF   ('&LXLV' NE '').LF12     BR IF LXLIST NOT SPECIFIED      14150000
&LXLV    SETC  '0'                      MUST BE ZERO IF NOT SPECIFIED   14200000
.LF12    ANOP                                                           14250000
         DC    A(&TKLV)             .TKLIST ADDRESS                     14300000
         DC    A(&LXLV)             .LXLIST ADDRESS                     14350000
         MEXIT                                                          14400000
.********************************************************************** 14450000
.*       EXECUTE FORM SPECIFIED                                       * 14500000
.********************************************************************** 14550000
.CKMF3   AIF   ('&MF(1)' NE 'E').ERR002 INVALID MF PARAMETER            14600000
         AIF   ('&MF(2)' EQ '').ERR002  MUST BE (E,PLIST ADDR)          14650000
.* DETERMINE WHETHER PARMLIST ADDRESS IS RX OR REG TYPE                 14700000
         AIF   ('&MF(2)'(1,1) EQ '(').EXREG   BRANCH IF REG TYPE        14750000
&EPLV    SETC  '&MF(2)'                 SET VALUE FOR USE IN LA INST.   14800000
&LT2     SETC  'A'                      SET TO 'LA' FOR RX TYPE ADDR    14850000
         AGO   .ETXT1                   GO GENERATE EXECUTE FORM TEXT   14900000
.EXREG   ANOP                                                           14950000
&LT2     SETC  'R'                      SET TO 'LR' FOR REG TYPE ADDR   15000000
&EPLV    SETC  '&MF(2)'                 EXTRACT REGISTER NAME           15050000
.ETXT1   ANOP                           GENERATE EXECUTE FORM TEXT      15100000
&LABL    DS    0H'0'                .ETCON XM SERVICE PC                15150000
         STM   14,12,12(13)         .SAVE CALLER'S REGS                 15200000
         AIF   ('&EPLV' EQ '1').ETXT3   BRANCH IF SPECIAL REG 1         15250000
.ETXT2   ANOP                                                           15300000
         L&LT2 1,&EPLV              .ETCON PARAMETER LIST ADDRESS       15350000
.ETXT3   AIF   (&OPTOI EQ 0).SKIPOI     IF NO OPTIONS TURNED ON         15400000
         OI    1(1),&OPTOI          .SET OPTIONS IN OPTIONS BYTE        15450000
.SKIPOI  AIF   (&OPTNI EQ 255).SKIPNI   IF NO OPTIONS TURNED OFF        15500000
         NI    1(1),&OPTNI          .CLEAR OPTIONS IN OPTIONS BYTE      15550000
.SKIPNI  AIF   (&S(9) NE 1).SKIPA1      IF TKLIST NOT SPECIFIED         15600000
         AIF   ('&LT1' EQ 'R').ETXT5    BR IF TKLIST IS REG FORM        15650000
         LA    2,&TKLV              .GET ADDR OF TKLIST                 15700000
         ST    2,4(,1)              .STORE TKLIST ADDR INTO PARM LIST   15750000
         AGO   .SKIPA1                                                  15800000
.ETXT5   ANOP                           REG FORM TKLIST                 15850000
         ST    &TKLV,4(,1)          .STORE TKLIST ADDR INTO PARM LIST   15900000
.*                                                                      15950000
.SKIPA1  AIF   (&S(10) NE 1).SKIPA2     IF LXLIST NOT SPECIFIED         16000000
         AIF   ('&LT3' EQ 'R').ETXT6    BR IF LXLIST IS REG FORM        16050000
         LA    2,&LXLV              .GET ADDR OF LXLIST                 16100000
         ST    2,8(,1)              .STORE LXLIST ADDR INTO PARM LIST   16150000
         AGO   .SKIPA2                                                  16200000
.ETXT6   ANOP                           REG FORM LXLIST                 16250000
         ST    &LXLV,8(,1)          .STORE LXLIST ADDR INTO PARM LIST   16300000
.SKIPA2  ANOP                                                           16350000
         ESAR  2                    .EXTRACT SASID                      16400000
         ST    2,16(,13)            .SAVE SASID IN REG 15 SLOT          16450000
         L     2,16(0,0)            .GET ADDR OF CVT                    16500000
         L     2,772(,2)            .GET ADDR OF SYSTEM FUNCTION TABLE  16550000
         L     2,16(,2)             .GET LX/EX FOR ETCON                16600000
         PC    0(2)                 .PC TO ETCON SERVICE                16650000
         L     2,16(,13)            .EXTRACT SAVED SASID                16700000
         SSAR  2                    .RESTORE SASID                      16750000
         LM    2,12,28(13)          .RESTORE REGS 2-12                  16800000
         L     14,12(,13)           .RESTORE REG 14                     16850000
         MEXIT                                                          16900000
.********************************************************************** 16950000
.*       PROCESS ERROR CONDITIONS                                     * 17000000
.********************************************************************** 17050000
.ERR001  ANOP SEV 12 MNOTE -- IHB001 &OPERAND REQ''D - NOT SPECIFIED    17100000
         IHBERMAC 1017,&OPERAND                                         17150000
         MEXIT                                                          17200000
.ERR002  ANOP SEV 12 MNOTE -- IHB002 - INVALID &OPERAND OPERAND &OPVAL  17250000
