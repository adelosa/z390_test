         MACRO                                                          03950000
&LABL    ETCON &TKLIST=,   REQUIRED RX OR REG LIST ADDR        (INPUT) X04000000
               &LXLIST=,   REQUIRED RX OR REG LXLIST ADDR      (INPUT) X04050000
               &MF=,       OPTIONAL MACRO FORM (DEFAULTS TO STANDARD)  X04100000
               &RELATED=   OPTIONAL VALUE FOR DOCUMENTATION             04150000
.********************************************************************** 04200000
.*       DECLARE ARITHMETIC VARIABLES                                 * 04250000
.********************************************************************** 04300000
         LCLA  &A          GENERAL USAGE LOCAL ARITH. VAR.              04350000
         LCLA  &OPTNI      MASK FOR NI OF OPTIONS BYTE                  04400000
         LCLA  &OPTOI      MASK FOR OI OF OPTIONS BYTE                  04450000
.********************************************************************** 04500000
.*       DECLARE BIT VARIABLES                                        * 04550000
.********************************************************************** 04600000
         LCLB  &B(8)       OPTIONS BITS                                 04650000
         LCLB  &N(8)       NI MASK BITS                                 04700000
         LCLB  &O(8)       OI MASK BITS                                 04750000
         LCLB  &S(10)      OPTIONS SPECIFIED BITS                       04800000
.********************************************************************** 04850000
.*       DECLARE CHARACTER VARIABLES                                  * 04900000
.********************************************************************** 04950000
         LCLC  &BOPTS      OPTIONS BYTE INIT STRING FOR STD. FORM       05000000
         LCLC  &EPLV       HOLDS E-FORM PARMADDR FOR USE IN LA/R INST.  05050000
         LCLC  &GNAME1     LABEL FOR STANDARD FORM                      05100000
         LCLC  &GNAME2     LABEL FOR STANDARD FORM                      05150000
         LCLC  &GNAMEL     LABEL FOR STANDARD FORM                      05200000
         LCLC  &LT1        LOAD TYPE 1 - THIS VARIABLE WILL BE:         05250000
.*                              'R' IF TKLIST IS A REG TYPE ADDR        05300000
.*                           OR 'A' IF TKLIST IS AN RX TYPE ADDR        05350000
         LCLC  &LT2        LOAD TYPE 2 - THIS VARIABLE WILL BE:         05400000
.*                              'R' IF E-FORM PARM IS A REG TYPE ADDR   05450000
.*                           OR 'A' IF E-FORM PARM IS AN RX TYPE ADDR   05500000
         LCLC  &LT3        LOAD TYPE 1 - THIS VARIABLE WILL BE:         05550000
.*                              'R' IF LXLIST  IS A REG TYPE ADDR       05600000
.*                           OR 'A' IF LXLIST  IS AN RX TYPE ADDR       05650000
         LCLC  &TKLV       HOLDS VALUE OF TKLIST FOR USE IN LA/R INST.  05700000
         LCLC  &LXLV       HOLDS VALUE OF LXLIST  FOR USE IN LA/R INST. 05750000
         LCLC  &OPERAND    HOLDS OPERAND STRINGS FOR MNOTES             05800000
         LCLC  &OPVAL      HOLDS OPERAND VALUES FOR MNOTES              05850000
.********************************************************************** 05900000
.*       INITIALIZE ARITHMETIC VARIABLES                              * 05950000
.********************************************************************** 06000000
&OPTNI   SETA  255      INIT TO TURN OFF NO OPTIONS                     06050000
&OPTOI   SETA  0        INIT TO TURN ON NO OPTIONS                      06100000
.********************************************************************** 06150000
.*       INITIALIZE BIT VARIABLES                                     * 06200000
.********************************************************************** 06250000
&B(1)    SETB  0        RESERVED BIT- MUST BE ZERO                      06300000
&B(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      06350000
&B(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      06400000
&B(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      06450000
&B(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      06500000
&B(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      06550000
&B(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      06600000
&B(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      06650000
.* IF AN NI-MASK BIT IS SET TO ZERO, THE NI WILL TURN OFF THE OPTION    06700000
&N(1)    SETB  1        RESERVED BIT- MUST BE ONE                       06750000
&N(2)    SETB  1        RESERVED BIT- MUST BE ONE                       06800000
&N(3)    SETB  1        RESERVED BIT- MUST BE ONE                       06850000
&N(4)    SETB  1        RESERVED BIT- MUST BE ONE                       06900000
&N(5)    SETB  1        RESERVED BIT- MUST BE ONE                       06950000
&N(6)    SETB  1        RESERVED BIT- MUST BE ONE                       07000000
&N(7)    SETB  1        RESERVED BIT- MUST BE ONE                       07050000
&N(8)    SETB  1        RESERVED BIT- MUST BE ONE                       07100000
.* IF AN OI-MASK BIT IS SET TO ONE, THE OI WILL TURN ON THE OPTION      07150000
&O(1)    SETB  0        RESERVED BIT- MUST BE ZERO                      07200000
&O(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      07250000
&O(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      07300000
&O(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      07350000
&O(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      07400000
&O(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      07450000
&O(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      07500000
&O(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      07550000
.*       AN 'S' BIT ON MEANS THE PARM WAS SPECIFIED                     07600000
&S(1)    SETB  0        RESERVED BIT- MUST BE ZERO                      07650000
&S(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      07700000
&S(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      07750000
&S(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      07800000
&S(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      07850000
&S(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      07900000
&S(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      07950000
&S(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      08000000
&S(9)    SETB  0   SHOW TKLIST NOT SPECIFIED                            08050000
&S(10)   SETB  0   SHOW LXLIST NOT SPECIFIED                            08100000
.*                                                                      08150000
.********************************************************************** 08200000
