         MACRO                                                          03550000
&LABL    LXFRE &LXLIST=,   REQUIRED RX OR REG FORM LIST ADDR   (INPUT) X03600000
               &FORCE=,    OPTIONAL, MUST BE 'YES' OR 'NO'     (INPUT) X03650000
               &MF=,       OPTIONAL MACRO FORM (DEFAULTS TO STANDARD)  X03700000
               &RELATED=   OPTIONAL VALUE FOR DOCUMENTATION             03750000
.*                                                                      03800000
.********************************************************************** 03850000
.*       DECLARE ARITHMETIC VARIABLES                                 * 03900000
.********************************************************************** 03950000
         LCLA  &A          GENERAL USAGE LOCAL ARITH. VAR.              04000000
         LCLA  &OPTNI      MASK FOR NI OF OPTIONS BYTE                  04050000
         LCLA  &OPTOI      MASK FOR OI OF OPTIONS BYTE                  04100000
.********************************************************************** 04150000
.*       DECLARE BIT VARIABLES                                        * 04200000
.********************************************************************** 04250000
         LCLB  &B(8)       OPTIONS BITS                                 04300000
         LCLB  &N(8)       NI MASK BITS                                 04350000
         LCLB  &O(8)       OI MASK BITS                                 04400000
         LCLB  &S(9)       OPTIONS SPECIFIED BITS                       04450000
.********************************************************************** 04500000
.*       DECLARE CHARACTER VARIABLES                                  * 04550000
.********************************************************************** 04600000
         LCLC  &BOPTS      OPTIONS BYTE INIT STRING FOR STD. FORM       04650000
         LCLC  &EPLV       HOLDS E-FORM PARMADDR FOR USE IN LA/R INST.  04700000
         LCLC  &GNAME1     LABEL FOR STANDARD FORM                      04750000
         LCLC  &GNAMEL     LABEL FOR STANDARD FORM                      04800000
         LCLC  &LT1        LOAD TYPE 1 - THIS VARIABLE WILL BE:         04850000
.*                              'R' IF LXLIST IS A REG TYPE ADDR        04900000
.*                           OR 'A' IF LXLIST IS AN RX TYPE ADDR        04950000
         LCLC  &LT2        LOAD TYPE 2 - THIS VARIABLE WILL BE:         05000000
.*                              'R' IF E-FORM PARM IS A REG TYPE ADDR   05050000
.*                           OR 'A' IF E-FORM PARM IS AN RX TYPE ADDR   05100000
         LCLC  &LXLV       HOLDS VALUE OF LXLIST FOR USE IN LA/R INST.  05150000
         LCLC  &OPERAND    HOLDS OPERAND STRINGS FOR MNOTES             05200000
         LCLC  &OPVAL      HOLDS OPERAND VALUES FOR MNOTES              05250000
         LCLC  &P1         CHAR VARIABLE FOR 'SYSTEM' PARM              05300000
.********************************************************************** 05350000
.*       INITIALIZE ARITHMETIC VARIABLES                              * 05400000
.********************************************************************** 05450000
&OPTNI   SETA  255      INIT TO TURN OFF NO OPTIONS                     05500000
&OPTOI   SETA  0        INIT TO TURN ON NO OPTIONS                      05550000
.********************************************************************** 05600000
.*       INITIALIZE BIT VARIABLES                                     * 05650000
.********************************************************************** 05700000
&B(1)    SETB  0   SHOW FORCE=NO                                        05750000
&B(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      05800000
&B(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      05850000
&B(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      05900000
&B(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      05950000
&B(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      06000000
&B(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      06050000
&B(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      06100000
.* IF AN NI-MASK BIT IS SET TO ZERO, THE NI WILL TURN OFF THE OPTION    06150000
&N(1)    SETB  1   NI-MASK BIT TO TURN OFF 'FORCE' OPTION               06200000
&N(2)    SETB  1        RESERVED BIT- MUST BE ONE                       06250000
&N(3)    SETB  1        RESERVED BIT- MUST BE ONE                       06300000
&N(4)    SETB  1        RESERVED BIT- MUST BE ONE                       06350000
&N(5)    SETB  1        RESERVED BIT- MUST BE ONE                       06400000
&N(6)    SETB  1        RESERVED BIT- MUST BE ONE                       06450000
&N(7)    SETB  1        RESERVED BIT- MUST BE ONE                       06500000
&N(8)    SETB  1        RESERVED BIT- MUST BE ONE                       06550000
.* IF AN OI-MASK BIT IS SET TO ONE, THE OI WILL TURN ON THE OPTION      06600000
&O(1)    SETB  0   OI-MASK BIT TO TURN ON 'FORCE' OPTION                06650000
&O(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      06700000
&O(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      06750000
&O(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      06800000
&O(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      06850000
&O(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      06900000
&O(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      06950000
&O(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      07000000
.*       AN 'S' BIT ON MEANS THE PARM WAS SPECIFIED                     07050000
&S(1)    SETB  0   SHOW FORCE NOT SPECIFIED                             07100000
&S(2)    SETB  0        RESERVED BIT- MUST BE ZERO                      07150000
&S(3)    SETB  0        RESERVED BIT- MUST BE ZERO                      07200000
&S(4)    SETB  0        RESERVED BIT- MUST BE ZERO                      07250000
&S(5)    SETB  0        RESERVED BIT- MUST BE ZERO                      07300000
&S(6)    SETB  0        RESERVED BIT- MUST BE ZERO                      07350000
&S(7)    SETB  0        RESERVED BIT- MUST BE ZERO                      07400000
&S(8)    SETB  0        RESERVED BIT- MUST BE ZERO                      07450000
&S(9)    SETB  0   SHOW LXLIST NOT SPECIFIED                            07500000
.*                                                                      07550000
.********************************************************************** 07600000
.*       PUT OUT BAL MACDATE COMMENT LINE REGARDLESS OF ERRORS.       * 07650000
.********************************************************************** 07700000
*              MACDATE = 87.118                                @YA04771 07750000
.********************************************************************** 07800000
.*       CHECK FOR EXTRANEOUS POSITIONAL PARMS                        * 07850000
.********************************************************************** 07900000
         AIF   (N'&SYSLIST NE 0).ERR027                                 07950000
.********************************************************************** 08000000
.*       VALIDATE LXLIST PARAMETER IF PRESENT                         * 08050000
.********************************************************************** 08100000
&S(9)    SETB  (T'&LXLIST NE 'O')       IF OPTION SPECIFIED SET BIT     08150000
         AIF   (T'&LXLIST EQ 'O').OK20  IF NOT, GO CHECK FORCE PARM     08200000
&OPERAND SETC  'LXLIST'                 SET FOR POSSIBLE ERROR MSGS     08250000
&OPVAL   SETC  '&LXLIST'                SET FOR POSSIBLE ERROR MSGS     08300000
.*   DETERMINE WHETHER LXLIST ADDRESS IS RX OR REG TYPE                 08350000
         AIF   ('&LXLIST'(1,1) EQ '(').OK11   BRANCH IF REG TYPE        08400000
&LXLV    SETC  '&LXLIST'                SET VALUE FOR USE IN LOAD INST. 08450000
&LT1     SETC  'A'                      SET TO 'LA' FOR RX TYPE ADDR    08500000
         AGO   .OK20                    GO CHECK FORCE PARAMETER        08550000
.OK11    ANOP                                                           08600000
&LT1     SETC  'R'                      SET TO 'LR' FOR REG TYPE ADDR   08650000
&LXLV    SETC  '&LXLIST(1)'             EXTRACT REGISTER NAME           08700000
         AIF   (N'&LXLIST NE 1).ERR002  BR IF INVALID REG FORM          08750000
.********************************************************************** 08800000
.*       VALIDATE FORCE PARAMETER IF PRESENT                          * 08850000
.********************************************************************** 08900000
.OK20    ANOP                                                           08950000
&S(1)    SETB  (T'&FORCE NE 'O')        SET OPTION SPECIFIED BIT        09000000
         AIF   (T'&FORCE EQ 'O').OK21                                   09050000
&OPERAND SETC  'FORCE'                                                  09100000
&OPVAL   SETC  '&FORCE'                                                 09150000
         AIF   ('&FORCE' NE 'YES' AND '&FORCE' NE 'NO').ERR002          09200000
&P1      SETC  '&FORCE'                                                 09250000
         AGO   .OK99                                                    09300000
.OK21    ANOP  DEFAULT TO FORCE=NO IF FORCE NOT SPECIFIED               09350000
.*             UNLESS IT IS THE EXECUTE FORM                            09400000
&P1      SETC  'NO'                                                     09450000
.OK99    ANOP                                                           09500000
.********************************************************************** 09550000
.* MAP OPTIONS BITS INTO OPTIONS BYTE                                   09600000
.********************************************************************** 09650000
&B(1)    SETB  ('&P1' EQ 'YES')                                         09700000
&BOPTS   SETC  '&B(1)&B(2)&B(3)&B(4)&B(5)&B(6)&B(7)&B(8)'               09750000
.MAP1    AIF   (&S(1) NE 1).MAP2        IF S(1)-FORCE WAS NOT SET       09800000
         AIF   (&B(1) EQ 1).MAP1S       IF FORCE=YES WAS SPECIFIED      09850000
.*                                      FORCE=NO   WAS SPECIFIED        09900000
&N(1)    SETB  0                        ENABLE EXECUTE FORM TO CLR BIT  09950000
         AGO   .MAP2                                                    10000000
.MAP1S   ANOP                                                           10050000
&O(1)    SETB  1                        ENABLE EXECUTE FORM TO SET BIT  10100000
.MAP2    ANOP                                                           10150000
&OPTNI   SETA  128*&N(1)+64*&N(2)+32*&N(3)+16*&N(4)+8*&N(5)+4*&N(6)+2*&X10200000
               N(7)+&N(8)               OPTIONS NI MASK VALUE           10250000
&OPTOI   SETA  128*&O(1)+64*&O(2)+32*&O(3)+16*&O(4)+8*&O(5)+4*&O(6)+2*&X10300000
               O(7)+&O(8)               OPTIONS OI MASK VALUE           10350000
.********************************************************************** 10400000
.*       DETERMINE TYPE OF REQUEST                                    * 10450000
.********************************************************************** 10500000
         AIF   (T'&MF NE 'O').CKMF2     MF OMITTED = STANDARD FORM      10550000
.********************************************************************** 10600000
.*       STANDARD FORM SPECIFIED                                      * 10650000
.********************************************************************** 10700000
&OPERAND SETC  'LXLIST'                                                 10750000
         AIF   ('&LXLV' EQ '').ERR001   REQUIRED OPERAND NOT SPECIFIED  10800000
.* GENERATE STANDARD FORM                                               10850000
&GNAME1  SETC  'IHB'.'&SYSNDX'.'1'                                      10900000
&GNAMEL  SETC  'IHB'.'&SYSNDX'.'L'                                      10950000
&LABL    DC    0H'0'                .LXFRE XM SERVICE PC                11000000
         STM   14,12,12(13)         .SAVE CALLER'S REGS                 11050000
         AIF   ('&LT1' EQ 'R').SF10     BR IF LXLIST WAS REG FORM       11100000
         LA    2,&LXLV              .GET ADDR OF LXLIST                 11150000
         ST    2,&GNAME1            .STORE LXLIST ADDR IN PARM LIST     11200000
         AGO   .SF12                                                    11250000
.SF10    ST    &LXLV,&GNAME1        .STORE LXLIST ADDR IN PARM LIST     11300000
.SF12    ANOP                                                           11350000
         CNOP  0,4                  .FORCE PARM LIST TO WORD BOUNDARY   11400000
         LA    1,*+8                .POINT REG1 TO PARM LIST            11450000
         B     &GNAMEL              .BRANCH AROUND PARM LIST            11500000
         DC    XL1'00'              .FORMAT BYTE                        11550000
         DC    BL1'&BOPTS'          .OPTIONS BYTE                       11600000
         DC    AL2(0)               .RESERVED                           11650000
&GNAME1  DC    A(0)                 .LXLIST ADDRESS                     11700000
&GNAMEL  ESAR  2                    .EXTRACT SASID                      11750000
         ST    2,16(,13)            .SAVE SASID IN REG 15 SLOT          11800000
         L     2,16(0,0)            .GET ADDR OF CVT                    11850000
         L     2,772(,2)            .GET ADDR OF SYSTEM FUNCTION TABLE  11900000
         L     2,4(,2)              .GET LX/EX FOR LXFRE                11950000
         PC    0(2)                 .PC TO LXFRE SERVICE                12000000
         L     2,16(,13)            .EXTRACT SAVED SASID                12050000
         SSAR  2                    .RESTORE SASID                      12100000
         LM    2,12,28(13)          .RESTORE REGS 2-12                  12150000
         L     14,12(,13)           .RESTORE REG 14                     12200000
         MEXIT                                                          12250000
.********************************************************************** 12300000
.CKMF2   ANOP                                                           12350000
&OPERAND SETC  'MF'                     SET FOR POSSIBLE ERROR MSGS     12400000
&OPVAL   SETC  '&MF'                                                    12450000
         AIF   ('&MF' NE 'L').CKMF3     IF NOT LIST FORM CK FOR EXECUTE 12500000
.********************************************************************** 12550000
.*                                                                      12600000
.********************************************************************** 12650000
.*       LIST FORM SPECIFIED                                          * 12700000
.********************************************************************** 12750000
&OPERAND SETC  'REG FORM LXLIST ADDRESS'                                12800000
&OPVAL   SETC  'MF=L'                                                   12850000
         AIF   ('&LT1' EQ 'R').ERR280   &OPERAND INVALID WITH &OPVAL    12900000
&LABL    DC    0F'0'                .LXFRE PC PARAMETER LIST            12950000
         DC    XL1'00'              .FORMAT BYTE                        13000000
         DC    BL1'&BOPTS'          .OPTIONS FLAG BYTE                  13050000
         DC    AL2(0)               .RESERVED                           13100000
         AIF   ('&LXLV' NE '').LF10     BR IF LXLIST WAS SPECIFIED      13150000
&LXLV    SETC  '0'                      DEFAULT TO ZERO                 13200000
.LF10    ANOP                                                           13250000
         DC    A(&LXLV)             .LXLIST ADDRESS                     13300000
         MEXIT                                                          13350000
.********************************************************************** 13400000
.*       EXECUTE FORM SPECIFIED                                       * 13450000
.********************************************************************** 13500000
.CKMF3   AIF   ('&MF(1)' NE 'E').ERR002 INVALID MF PARAMETER            13550000
         AIF   ('&MF(2)' EQ '').ERR002  MUST BE (E,PLIST ADDR)          13600000
.* DETERMINE WHETHER PARMLIST ADDRESS IS RX OR REG TYPE                 13650000
         AIF   ('&MF(2)'(1,1) EQ '(').EXREG      BRANCH IF REG TYPE     13700000
&EPLV    SETC  '&MF(2)'                 SET VALUE FOR USE IN LOAD INST. 13750000
&LT2     SETC  'A'                      SET TO 'LA' FOR RX TYPE ADDR    13800000
         AGO   .ETXT1                   GO GENERATE EXECUTE FORM TEXT   13850000
.EXREG   ANOP                                                           13900000
&LT2     SETC  'R'                      SET TO 'LR' FOR REG TYPE ADDR   13950000
&EPLV    SETC  '&MF(2)'(2,K'&MF(2)-2)   EXTRACT REGISTER NAME           14000000
.ETXT1   ANOP                           GENERATE EXECUTE FORM TEXT      14050000
&LABL    DC    0H'0'                .LXFRE XM SERVICE PC                14100000
         STM   14,12,12(13)         .SAVE CALLER'S REGS                 14150000
         AIF   ('&EPLV' EQ '1').ETXT3   BRANCH IF SPECIAL REG 1         14200000
.ETXT2   ANOP                                                           14250000
         L&LT2 1,&EPLV              .LXFRE PARAMETER LIST ADDRESS       14300000
.ETXT3   ANOP                                                           14350000
         AIF   (&OPTOI EQ 0).SKIPOI     IF NO OPTIONS TURNED ON         14400000
         OI    1(1),&OPTOI          .SET OPTIONS IN OPTIONS BYTE        14450000
.SKIPOI  AIF   (&OPTNI EQ 255).SKIPNI   IF NO OPTIONS TURNED OFF        14500000
         NI    1(1),&OPTNI          .CLEAR OPTIONS IN OPTIONS BYTE      14550000
.SKIPNI  AIF   (&S(9) NE 1).SKIPLX      IF LXLIST NOT SPECIFIED         14600000
         AIF   ('&LT1' EQ 'R').ETXT5  BR IF LXLIST IS REG FORM @YA04771 14650000
         LA    2,&LXLV              .GET ADDR OF LXLIST                 14700000
         ST    2,4(,1)              .STORE LXLIST ADDR INTO PARM LIST   14750000
         AGO   .SKIPLX                                                  14800000
.ETXT5   ANOP                           REG FORM LXLIST                 14850000
         ST    &LXLV,4(,1)          .STORE LXLIST ADDR INTO PARM LIST   14900000
.SKIPLX  ANOP                                                           14950000
         ESAR  2                    .EXTRACT SASID                      15000000
         ST    2,16(,13)            .SAVE SASID IN REG 15 SLOT          15050000
         L     2,16(0,0)            .GET ADDR OF CVT                    15100000
         L     2,772(,2)            .GET ADDR OF SYSTEM FUNCTION TABLE  15150000
         L     2,4(,2)              .GET LX/EX FOR LXFRE                15200000
         PC    0(2)                 .PC TO LXFRE SERVICE                15250000
         L     2,16(,13)            .EXTRACT SAVED SASID                15300000
         SSAR  2                    .RESTORE SASID                      15350000
         LM    2,12,28(13)          .RESTORE REGS 2-12                  15400000
         L     14,12(,13)           .RESTORE REG 14                     15450000
         MEXIT                                                          15500000
.********************************************************************** 15550000
.*       PROCESS ERROR CONDITIONS                                     * 15600000
.********************************************************************** 15650000
.ERR001  ANOP SEV 12 MNOTE -- IHB001 &OPERAND REQ''D - NOT SPECIFIED    15700000
         IHBERMAC 1017,&OPERAND                                         15750000
         MEXIT                                                          15800000
.ERR002  ANOP SEV 12 MNOTE -- IHB002 - INVALID &OPERAND OPERAND &OPVAL  15850000
         IHBERMAC 1001,&OPERAND,&OPVAL                                  15900000
         MEXIT                                                          15950000
.ERR027  ANOP SEV 12 MNOTE -- IHB027 - EXCESSIVE POSITIONAL PARAMETERS  16000000
         IHBERMAC 238                                                   16050000
         MEXIT                                                          16100000
.ERR280  ANOP SEV 12 MNOTE -- IHB280 &OPERAND INVALID WITH &OPVAL       16150000
         IHBERMAC 1020,&OPERAND,&OPVAL                                  16200000
         MEND                                                           16250000
