         B     1(,15)                     +16 entry:  Exception!        00322000
         ORG   ,                                                        00323000
         DS    0H                                                       00324000
         SPACE 1                                                        00325000
CEEYCD00 LR    0,14                SAVE LINK REGISTER                   00326000
         LTR   1,1                 PARAMETER PROVIDED ?                 00327000
         BZ    CEEYCD03                                                 00328000
         LA    14,CEEQC0MT         ADDRESS LOGIC MODULE TABLE           00329000
CEEYCD01 DS    0H                                                       00330000
         CLI   0(14),X'FF'         END OF TABLE ?                       00331000
         BE    CEEYCD03            YES ...                              00332000
         CLC   0(3,14),KIJC        VALID PHASE NAME IN TABLE ?          00333000
         BNE   CEEYCD04            NO ...                               00334000
         CLC   0(8,14),0(1)        LOGIC MODULE FOUND ?                 00335000
         BE    CEEYCD02            YES ...                              00336000
         LA    14,12(14)           NEXT TABLE ENTRY                     00337000
         B     CEEYCD01            LOOP                                 00338000
CEEYCD02 DS    0H                                                       00339000
         L     1,8(14)             LOAD ADDRESS OF LOGIC MODULE         00340000
         XR    14,14               RETURN CODE = OK                     00341000
         B     CEEYCD05                                                 00342000
CEEYCD03 DS    0H                                                       00343000
         XR    1,1                 CLEAR LOGIC MODULE ADDRESS           00344000
         LA    14,4                RC = LOGIC MODULE NOT FOUND          00345000
         B     CEEYCD05                                                 00346000
CEEYCD04 DS    0H                                                       00347000
         XR    1,1                 CLEAR LOGIC MODULE ADDRESS           00348000
         LA    14,8                RC = INVALID MODULE NAME IN TABLE    00349000
CEEYCD05 DS    0H                                                       00350000
         LR    15,14               SET RETURN CODE IN REGISTER 15       00351000
         LR    14,0                RESTORE LINK REGISTER                00352000
         BR    14                  RETURN TO CALLER                     00353000
KIJC     DC    CL3'IJC'            LOGIC MODULE NAME PREFIX             00354000
         EJECT                                                          00355000
CEEQC0MT DS    0H                  START OF LOGIC MODULE TABLE          00356000
         AGO   .ENDGEN                                                  00357000
