.***  TYPE = ENTRY (PROCESS MACRO PARAMETERS)                                   
.*                                                                              
.ENTRY   ANOP                                                                   
         AIF   (&TYPESW EQ 1).TYPERR                                            
         AIF   (&TYPESW2 EQ 1).SKIPORG  SKIP IF FOLLOWING DONE                  
         DC    A(FINAL-FINAL)  TYPE=FINAL INTEGRITY CHECK                       
* IF PRECEDING ASSEMBLES IN ERROR, TYPE=FINAL WAS OMITTED,                      
* REQUIRED WHEN TYPE=ENTRY USED                                                 
         ORG   *-4                                                              
&TYPESW2 SETA  1               INDICATE TYPE=FINAL CHECK DONE                   
.SKIPORG ANOP  ,                                                                
         AIF   (T'&LANGKEY    EQ 'O').TSTFWD                                    
         MNOTE ,'''LANGKEY'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTFWD  ANOP  ,                                                                
         AIF   (T'&FORWARD(1) EQ 'O').TSTBWD                                    
         MNOTE ,'''FORWARD'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTBWD  ANOP                                                                   
         AIF   (T'&BACWARD(1) EQ 'O').TSTREF                                    
         MNOTE ,'''BACWARD'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTREF  ANOP                                                                   
         AIF   (T'&REFRESH(1) EQ 'O').TSTTIM                                    
         MNOTE ,'''REFRESH'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTTIM  ANOP                                                                   
         AIF   (T'&TIME EQ 'O').TSTMSGN                                         
         MNOTE ,'''TIME'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASSEMB        
               BLY'                                                             
.TSTMSGN ANOP                                                                   
         AIF   (T'&MSGFILE EQ 'O').TSTEDTC                                      
         MNOTE ,'''MSGFILE'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTEDTC ANOP                                                                   
         AIF   (T'&EDTRCMD EQ 'O').TSTEDTP                                      
         MNOTE ,'''EDTRCMD'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTEDTP ANOP                                                                   
         AIF   (T'&EDTRPFK(1) EQ 'O').TSTEDTI                                   
         MNOTE ,'''EDTRPFK'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASS        
               SEMBLY'                                                          
.TSTEDTI ANOP                                                                   
         AIF   (T'&EDTRID EQ 'O').TSTFILN                                       
         MNOTE ,'''EDTRID'' CAN ONLY BE SPECIFIED ON FIRST MACRO IN ASSE        
               EMBLY'                                                           
.TSTFILN ANOP                                                                   
         AIF   (T'&FILENAM NE 'O').FILLENG                                      
         MNOTE ,'''FILENAM'' HAS BEEN OMITTED - CORRECT AND REASSEMBLE'         
         AGO   .TRANID                                                          
.FILLENG ANOP                                                                   
         AIF   (K'&FILENAM LE 7).FILEOK                                         
         MNOTE ,'''FILENAM'' VALUE IS TOO LONG - CORRECT AND REASSEMBLE'        
               '                                                                
         AGO   .TRANID                                                          
.FILEOK  ANOP                                                                   
&DCFILNM SETC  '&FILENAM'                                                       
.TRANID  ANOP                                                                   
         AIF   (T'&TRANID NE 'O').ALTTRAN                                       
         MNOTE ,'''TRANID'' HAS BEEN OMITTED - CORRECT AND REASSEMBLE'          
         AGO   .DOSREL                                                          
.ALTTRAN ANOP                                                                   
         AIF   (K'&TRANID LE 4).ALTRNOK                                         
         MNOTE ,'''TRANID'' VALUE IS TOO LONG - CORRECT AND REASSEMBLE'         
         AGO   .DOSREL                                                          
.ALTRNOK ANOP                                                                   
&DCTRAN  SETC  '&TRANID'                                                        
.DOSREL  ANOP                                                                   
         AIF   (T'&DOSREL EQ 'O').RELSERR                                       
.*       X'28' = RELEASE 34                                                     
&DCDOSRL SETC  '28'                                                             
         AIF   ('&DOSREL' EQ '34').GENCODE                                      
.*       X'29' = VSE RELEASE 1                                                  
&DCDOSRL SETC  '29'                                                             
         AIF   ('&DOSREL' EQ '35').GENCODE                                      
         AIF   ('&DOSREL' EQ 'VSE1').GENCODE                                    
.*       X'2A' = VSE RELEASE 2                                                  
&DCDOSRL SETC  '2A'                                                             
         AIF   ('&DOSREL' EQ '36').GENCODE                                      
         AIF   ('&DOSREL' EQ 'VSE2').GENCODE                                    
.*       X'2B' = VSE RELEASE 3                                                  
&DCDOSRL SETC  '2B'                                                             
         AIF   ('&DOSREL' EQ '37').GENCODE                                      
         AIF   ('&DOSREL' EQ 'VSE3').GENCODE                                    
.*       X'2C' = VSE RELEASE 3.5                                                
&DCDOSRL SETC  '2C'                                                             
         AIF   ('&DOSREL' EQ '37').GENCODE                                      
         AIF   ('&DOSREL' EQ 'VSE3.5').GENCODE                                  
.*       X'2D' = VSE RELEASE SP 2.1                                             
&DCDOSRL SETC  '2D'                                                             
         AIF   ('&DOSREL' EQ '38').GENCODE                                      
         AIF   ('&DOSREL' EQ 'SP2.1').GENCODE                                   
         AIF   ('&DOSREL' EQ '39').GENCODE                                      
         AIF   ('&DOSREL' EQ 'SP3.1').GENCODE                                   
         AIF   ('&DOSREL' EQ '40').GENCODE                                      
         AIF   ('&DOSREL' EQ 'SP3.2').GENCODE                                   
         AIF   ('&DOSREL' EQ 'SP4.1').GENCODE                                   
.*       X'35' = VSE RELEASE FOR ESA 1.0-1.2                                    
&DCDOSRL SETC  '35'                                                             
         AIF   ('&DOSREL' EQ 'ESA').GENCODE                        2.8A         
         AIF   ('&DOSREL' EQ 'ESA1.0').GENCODE                                  
.*       X'36' = VSE RELEASE FOR ESA 1.3.X                         2.8B         
&DCDOSRL SETC  '36'                                                2.8B         
         AIF   ('&DOSREL' EQ 'ESA1.3').GENCODE                     2.8B         
.*       X'37' = VSE RELEASE FOR ESA 2.1       ??????              2.8B         
&DCDOSRL SETC  '37'                                                2.8B         
         AIF   ('&DOSREL' EQ 'ESA2.1').GENCODE                     2.8B         
.RELSERR ANOP                                                                   
&DCDOSRL SETC  '00'                DOS RELEASE IS INVALID                       
.GENCODE ANOP                                                                   
         DC    CL4'&DCTRAN'        FILE TRANSACTION ID                          
         DC    CL7'&DCFILNM'       HARDCOPY FILE NAME                           
         DC    XL1'00'             INDICATOR FLAG                               
         DC    F'0'                HIGH EXTENT LIMIT FOR IJSYSCN                
         DC    XL8'00'             SEEK ADDRESS FOR DISK READ                   
         DC    XL8'00'             CURRENT POSITION ADDRESS                     
         DC    F'0'                TASK SELECTION STRING ADDRESS                
         DC    F'0'                LOW EXTENT LIMIT FOR IJSYSCN                 
         DC    XL1'00'             LOW LIMIT CKD RECORD                         
         DC    XL1'00'             CONSOLE DEVICE TYPE                          
         DC    H'0'                IJSYSCN BLOCK SIZE                           
         DC    H'0'                CONSOLE FILE RECORD SIZE                     
         DC    H'0'                CONSOLE FILE I/O BUFFER SIZE                 
         DC    H'0'                CONSOLE SYS-NUMBER                           
         DC    XL1'00'             RECORDS PER TRACK                            
         DC    XL1'00'             TRACKS PER CYLINDER                          
         DC    XL1'&DCDOSRL'       DOS RELEASE INDICATOR                        
         DC    CL2'&GTIME'         NUMBER OF SECONDS BEFORE RE-DISPLAY          
         DC    AL2(&SCNLIM)        END SEARCH/ARG NOT FOUND AFTER LINES         
         DC    CL1&PFRCL           PF KEY RECALL                                
         DC    CL1&PFRCL2          PF KEY RECALL - ALTERNATE                    
         DC    CL1&PFFWD           PF KEY FORWARD                               
         DC    CL1&PFBWD           PF KEY BACKWARD                              
         DC    CL1&PFREF           PF KEY REFRESH                               
         DC    CL1&PFFWD2          PF KEY FORWARD - ALTERNATE                   
         DC    CL1&PFBWD2          PF KEY BACKWARD - ALTERNATE                  
         DC    CL1&PFREF2          PF KEY REFRESH - ALTERNATE                   
         DC    CL7'&MSGFIL'        FILE NAME FOR ONLINE MESSAGE FILE            
         DC    CL16'  (&PFFO,&PFFO2)FWD'                                        
         DC    CL16'  (&PFBA,&PFBA2)BWD'                                        
         DC    CL16'  (&PFEND,&PFEND2)END'                                      
         DC    XL1'&CMDSEQX'       COMMAND SEQUENCE.                            
         DC    CL4'&EDTRC'         RETURN TO BIM-EDIT COMMAND                   
         DC    CL1&PFEDT           RETURN TO BIM-EDIT PF KEY                    
         DC    CL1&PFEDT2          RETURN TO BIM-EDIT PF KEY - ALT.             
         DC    CL4'&EDTRI'         RETURN TO BIM-EDIT TRANSID                   
         DC    CL1'&LANGKYL'       MSG FILE LANGUAGE KEY PREFIX                 
         DC    CL1&PFMSG           PF KEY MESSAGE ACCESS            2.8         
         DC    CL1&PFMSG2          PF KEY MESSAGE ACCESS - ALT      2.8         
         DC    CL1&PFRFI           PF KEY REPEAT FIND               2.8         
         DC    CL1&PFRFI2          PF KEY REPEAT FIND    - ALT      2.8         
         DC    XL8'00'             FUTURE                           2.8         
         AIF   (&TYPESW EQ 1).FOXES                                             
         MEXIT                                                                  
