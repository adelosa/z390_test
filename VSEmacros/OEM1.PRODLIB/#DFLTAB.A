.DROPIN  ANOP                                                       #B1         
         AIF   (&N LT 1).DROPOUT                                    #B1         
         AIF   ('&DROPSESS'(&N,1) LT '0').DROPER1  DIGIT < 0 ?      #B1         
         AIF   ('&DROPSESS'(&N,1) GT '9').DROPER1  DIGIT > 9 ?      #B1         
&N       SETA  &N-1                                                 #B1         
         AGO   .DROPIN                                              #B1         
.DROPOUT ANOP                                                       #B1         
&N       SETA  &DROPSESS                                            #B1         
         AIF   ( &N LT 0 ).DROPER1                                  #B1         
         AIF   ( &N GT 60 ).DROPER2                                 #B1         
&DDRPSES SETC  '&N'                    DROPSESS= TIME IN MINUTES    #B1         
         AGO   .PDRPSER                                             #B1         
.DROPER1 ANOP                                                       #B1         
&DDRPSES SETC  '0'                     NULL GLOBAL DEFAULT DROPSESS #B1         
         MNOTE 4,'XCOME0651E PARAMETER "&ERRPARM", VALUE "&ERRDATA", ISX        
                INVALID AND'                                        #B1         
         MNOTE 4,'           WAS RESET TO NULL - NO DEFAULT TABLE VALUEX        
                ASSIGNED.'                                          #B1         
         AGO   .PDRPSER                                             #B1         
.DROPER2 ANOP                                                       #B1         
&DDRPSES SETC  '60'                    MAX DROPSESS TIME INTERVAL   #B1         
         MNOTE 4,'XCOME0651E PARAMETER "&ERRPARM", VALUE "&ERRDATA", ISX        
                GREATER THAN'                                       #B1         
         MNOTE 4,'           MAXIMUM ALLOWED, RESET TO "&DDRPSES".' #B1         
         AGO   .PDRPSER                                             #B1         
.*                                                                  #AO         
.PDRPSEG ANOP                                                       #AO         
&DDRPSES SETC  'C'''.'&DROPSESS'(1,1).''''                          #B1         
         AGO   .PDRPSER                                             #AO         
