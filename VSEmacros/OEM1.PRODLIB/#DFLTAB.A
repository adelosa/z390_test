.*******************************************************************#AI         
.****                     PSO PREFIX                            ****#AI         
.*******************************************************************#AI         
.*                                                                  #AI         
.PPSOPRE ANOP                                                       #AI         
&ERRPARM SETC  'PSOPREF'                                            #AI         
&ERRDATA SETC  '&PSOPREF'                                           #AI         
         AIF   (K'&PSOPREF GT 20).PLENERR                           #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*   VALIDATION STRINGS                                            *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
&VS1     SETC  'ABCDEFGHIJKLMNOPQRSTUVWXYZ@#$'                      #A9         
&VS2     SETC  'ABCDEFGHIJKLMNOPQRSTUVWXYZ@#$0123456789'            #A9         
.*                                                                  #A9         
&L1      SETA  K'&VS1                    VALIDATION STRING 1 LENGTH #A9         
&L2      SETA  K'&VS2                    VALIDATION STRING 2 LENGTH #A9         
.*                                                                  #A9         
&L       SETA  K'&PSOPREF                LENGTH OF SUPPLIED VALUE   #A9         
&IX      SETA  0                         INDEX INTO PSOPREF         #A9         
&DOT     SETB  1                         FLAG INDICATES A '.' FOUND #A9         
&DIX     SETA  0                         INDEX OF LAST '.'          #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*  LOOP - VALIDATING EACH CHARACTER OF THE DSN                    *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
.PSOLOOP ANOP  ,                                                    #A9         
.*                                                                  #A9         
&IX      SETA  &IX+1                     INDEX INTO PREFIX          #A9         
.*                                                                  #A9         
         AIF   (&IX GT &L).DPSOPRE       WE ARE DONE...             #A9         
         AIF   (&DOT).PSOFST             LAST CHARACTER A '.'?      #A9         
         AIF   ('&PSOPREF'(&IX,1) EQ '.').PSOSEG   IS THIS A '.'??? #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*  VALIDATE 2ND - N CHARACTER OF A DSN SEGMENT                    *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
&X1      SETA  1                         VALIDATION STRING INDEX    #A9         
.*                                                                  #A9         
.PSLOOP1 ANOP  ,                                                    #A9         
.*                                                                  #A9         
         AIF   ('&PSOPREF'(&IX,1) EQ '&VS2'(&X1,1)).PSOLOOP         #A9         
.*                                                                  #A9         
&X1      SETA  &X1+1                     INCREMENT INDEX            #A9         
.*                                                                  #A9         
         AIF   (&X1 GT &L2).PERROUT      VALIDATION STRING EXHAUSTED#A9         
.*                                                                  #A9         
         AGO   .PSLOOP1                  LOOP                       #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*  VALIDATE FIRST CHARACTER OF DSN SEGMENT                        *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
.PSOFST  ANOP  ,                                                    #A9         
.*                                                                  #A9         
&DOT     SETB  0                         RESET '.' FOUND FLAG       #A9         
&X1      SETA  1                         VALIDATION STRING INDEX    #A9         
.*                                                                  #A9         
         AIF   ('&PSOPREF'(&IX,1) NE '.').PSLOOP2                   #A9         
&DIX     SETA  &IX                       INDEX OF LAST '.'          #A9         
.*                                                                  #A9         
.PSLOOP2 ANOP  ,                                                    #A9         
.*                                                                  #A9         
         AIF   ('&PSOPREF'(&IX,1) EQ '&VS1'(&X1,1)).PSOLOOP         #A9         
.*                                                                  #A9         
&X1      SETA  &X1+1                     INCREMENT INDEX            #A9         
.*                                                                  #A9         
         AIF   (&X1 GT &L1).PERROUT      VALIDATION STRING EXHAUSTED#A9         
.*                                                                  #A9         
         AGO   .PSLOOP2                  LOOP                       #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*  A '.' WAS FOUND IN THE PSO PREFIX, START A NEW "SEGMENT"       *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
.PSOSEG  ANOP  ,                                                    #A9         
.*                                                                  #A9         
         AIF   (&IX EQ (&DIX+1)).PERROUT '..' IS INVALID            #A9         
.*                                                                  #A9         
&DOT     SETB  1                         SET '.' FOUND FLAG         #A9         
.*                                                                  #A9         
         AGO   .PSOLOOP                  LOOP                       #A9         
.*                                                                  #A9         
.*******************************************************************#A9         
.*       PSO PREFIX VALIDATED...                                   *#A9         
.*******************************************************************#A9         
.*                                                                  #A9         
.DPSOPRE ANOP  ,                                                    #A9         
.*                                                                  #A9         
&DPSOPRE SETC  '&PSOPREF'                                           #AI         
         AGO   .PPSOPRR                                             #AI         
.*                                                                              
.*******************************************************************#AJ         
.****                         MAXRPTB                           ****#AJ         
.*******************************************************************#AJ         
.*                                                                  #AJ         
.PMAXRPT ANOP                                                       #AJ         
&ERRPARM SETC  'MAXRPTB'                                            #AK         
&ERRDATA SETC  '&MAXRPTB'                                           #AK         
&DMAXRPT SETC  '&MAXRPTB'                                           #AM         
         AIF   ('&MAXRPTB' GE '512').PMRB01                         #AR         
         MNOTE 4,'XCOME0651E PARAMETER &ERRPARM VALUE &ERRDATA INVALID X        
               SET TO 512         -- MIN VALUE'                     #AR         
.**#AJ &DMXRBSZ SETC  '512'                                         #AR         
&DMAXRPT SETC  '512'                                                #AR         
         AGO   .PMRB02                                              #AJ         
.PMRB01  ANOP                                                       #AJ         
         AIF   ('&MAXRPTB' LE '32760').PMRB02                       #AJ         
         MNOTE 4,'XCOME0651E PARAMETER &ERRPARM VALUE &ERRDATA INVALID X        
               SET TO 32760       -- MAX VALUE'                     #AK         
&DMAXRPT SETC  '32760'                                              #AJ         
         AGO   .PMRB02                                              #AJ         
.*                                                                  #AJ         
.PMRB02  ANOP                                                       #AJ         
.**#AJ &DMAXRPT SETC  '&MAXRPTB'                                    #AM         
         AGO   .PMRBSZR                    GO PROCESS NEXT PARAM    #AJ         
.*******************************************************************#AT         
.****                         ALERTS                            ****#AT         
.*******************************************************************#AT         
.PALERT  ANOP                                                       #AT         
.*                                                                  #AT         
.*   SET SWITCHES                                                   #AT         
.*                                                                  #AT         
&CONVF   SETB   0                                                   #AT         
&SESSF   SETB   0                                                   #AT         
&GENF    SETB   0                                                   #AT         
&SECF    SETB   0                                                   #AT         
&FILEF   SETB   0                                                   #AT         
&ALLF    SETB   0                                                   #AT         
.*                                                                  #AT         
&NA      SETA   (N'&ALERTS)           NUMBER OF OPERANDS            #AT         
&I       SETA   0                     INDEX                         #AT         
.*                                                                  #AT         
.LOOP    ANOP                                                       #AT         
&I       SETA   &I+1                  INCREMENT INDEX               #AT         
         AIF    (&I GT &NA).PALERTR   ALL OPERANDS PROCESSED        #AT         
.*                                                                  #AT         
&A       SETC  '&ALERTS(&I)'          ALERT CATEGORY I              #AT         
&L       SETA  K'&ALERTS(&I)          LENGTH OF ALERT CATEGORY I    #AT         
.*                                                                  #AT         
         AIF   ('&ALERTS(&I)'(1,4)  EQ 'CONV').CONV                 #AT         
         AIF   ('&ALERTS(&I)'(1,12) EQ 'CONVERSATION').CONV         #AT         
         AIF   ('&ALERTS(&I)'(1,4)  EQ 'SESS').SESS                 #AT         
         AIF   ('&ALERTS(&I)'(1,8)  EQ 'SESSION').SESS              #AT         
         AIF   ('&ALERTS(&I)'(1,3)  EQ 'GEN').GEN                   #AT         
         AIF   ('&ALERTS(&I)'(1,7)  EQ 'GENERAL').GEN               #AT         
         AIF   ('&ALERTS(&I)'(1,3)  EQ 'SEC').SEC                   #AT         
         AIF   ('&ALERTS(&I)'(1,8)  EQ 'SECURITY').SEC              #AT         
         AIF   ('&ALERTS(&I)'(1,4)  EQ 'FILE').FILE                 #AT         
         AIF   ('&ALERTS(&I)'(1,3)  EQ 'ALL').ALL                   #AT         
         AGO   .AERR                                                #AT         
.*                                                                  #AT         
.*  PROCESS CONVERSATION CATEGORY.                                  #AT         
.*                                                                  #AT         
.CONV    ANOP                                                       #AT         
         AIF    (&CONVF).ADUP                                       #AT         
&CONVF   SETB  1                                                    #AT         
&ARETN   SETA  1                                                    #AT         
         AGO   .PSCAN                                               #AT         
.CONVR   ANOP                                                       #AT         
&DFLACON SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  PROCESS SESSION CATEGORY.                                       #AT         
.*                                                                  #AT         
.SESS    ANOP                                                       #AT         
         AIF    (&SESSF).ADUP                                       #AT         
&SESSF   SETB  1                                                    #AT         
&ARETN   SETA  2                                                    #AT         
         AGO   .PSCAN                                               #AT         
.SESSR   ANOP                                                       #AT         
&DFLASES SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  PROCESS GENERAL CATEGORY.                                       #AT         
.*                                                                  #AT         
.GEN     ANOP                                                       #AT         
         AIF    (&GENF).ADUP                                        #AT         
&GENF    SETB  1                                                    #AT         
&ARETN   SETA  3                                                    #AT         
         AGO   .PSCAN                                               #AT         
.GENR    ANOP                                                       #AT         
&DFLAGEN SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  PROCESS SECURITY CATEGORY.                                      #AT         
.*                                                                  #AT         
.SEC     ANOP                                                       #AT         
         AIF    (&SECF).ADUP                                        #AT         
&SECF    SETB  1                                                    #AT         
&ARETN   SETA  4                                                    #AT         
         AGO   .PSCAN                                               #AT         
.SECR    ANOP                                                       #AT         
&DFLASEC SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  PROCESS FILE CATEGORY.                                          #AT         
.*                                                                  #AT         
.FILE    ANOP                                                       #AT         
         AIF    (&FILEF).ADUP                                       #AT         
&FILEF   SETB  1                                                    #AT         
&ARETN   SETA  5                                                    #AT         
         AGO   .PSCAN                                               #AT         
.FILER   ANOP                                                       #AT         
&DFLAFIL SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  PROCESS ALL CATEGORY.                                           #AT         
.*                                                                  #AT         
.ALL     ANOP                                                       #AT         
         AIF   (&ALLF).ADUP                                         #AT         
.*       AIF   (&NA GT 1).ALLERR                                    #AT         
&ALLF    SETB  1                                                    #AT         
&ARETN   SETA  6                                                    #AT         
         AGO   .PSCAN                                               #AT         
.ALLR    ANOP                                                       #AT         
&DFLACON SETC  '&AW'                                                #AT         
&DFLASES SETC  '&AW'                                                #AT         
&DFLAGEN SETC  '&AW'                                                #AT         
&DFLASEC SETC  '&AW'                                                #AT         
&DFLAFIL SETC  '&AW'                                                #AT         
         AGO    .LOOP                                               #AT         
.*                                                                  #AT         
.*  .PSCAN SCANS OPERANDS OF THE ALERTS= KEYWORD TO DETERMINE       #AT         
.*  THE APPROPRIATE LEVEL TO BE SET.                                #AT         
.*                                                                  #AT         
.PSCAN   ANOP                                                       #AT         
&INDEXL  SETA   1                                                   #AT         
.*                                                                  #AT         
.LPSCAN  ANOP                                                       #AT         
         AIF    ('&A'(&INDEXL,1) EQ '(').LPAREN                     #AT         
&INDEXL  SETA   &INDEXL+1                                           #AT         
         AIF    (&INDEXL LE &L).LPSCAN                              #AT         
         AGO    .AE                                                 #AT         
.*                                                                  #AT         
.LPAREN  ANOP                                                       #AT         
&INDEXR  SETA   &INDEXL+1                                           #AT         
.RPSCAN  AIF    ('&A'(&INDEXR,1) EQ ')').RPAREN                     #AT         
&INDEXR  SETA   &INDEXR+1                                           #AT         
         AGO    .RPSCAN                                             #AT         
.*                                                                  #AT         
.RPAREN  ANOP                                                       #AT         
&TA      SETC   '&A'(&INDEXL+1,&INDEXR-&INDEXL-1)                   #AT         
         AIF    ('&TA' EQ 'INFORMATION').AI                         #AT         
         AIF    ('&TA' EQ 'INFO').AI                                #AT         
         AIF    ('&TA' EQ 'I').AI                                   #AT         
         AIF    ('&TA' EQ 'WARNING').AW                             #AT         
         AIF    ('&TA' EQ 'WARN').AW                                #AT         
         AIF    ('&TA' EQ 'W').AW                                   #AT         
         AIF    ('&TA' EQ 'ERROR').AE                               #AT         
         AIF    ('&TA' EQ 'E').AE                                   #AT         
         AIF    ('&TA' EQ 'ACTION').AA                              #AT         
         AIF    ('&TA' EQ 'A').AA                                   #AT         
         AIF    ('&TA' EQ 'NONE').AN                                #AT         
         AIF    ('&TA' EQ 'N').AN                                   #AT         
         AGO    .AERR                                               #AT         
.AI      ANOP                                                       #AT         
&AW      SETC   'F0'                                                #AT         
         AGO   .ARETURN                                             #AT         
.AW      ANOP                                                       #AT         
&AW      SETC   '70'                                                #AT         
         AGO   .ARETURN                                             #AT         
.AE      ANOP                                                       #AT         
&AW      SETC   '30'                                                #AT         
         AGO   .ARETURN                                             #AT         
.AA      ANOP                                                       #AT         
&AW      SETC   '10'                                                #AT         
         AGO   .ARETURN                                             #AT         
.AN      ANOP                                                       #AT         
&AW      SETC   '00'                                                #AT         
         AGO   .ARETURN                                             #AT         
.*                                                                  #AT         
.ARETURN ANOP                                                       #AT         
         AIF   (&ARETN EQ 1).CONVR                                  #AT         
         AIF   (&ARETN EQ 2).SESSR                                  #AT         
         AIF   (&ARETN EQ 3).GENR                                   #AT         
         AIF   (&ARETN EQ 4).SECR                                   #AT         
         AIF   (&ARETN EQ 5).FILER                                  #AT         
         AIF   (&ARETN EQ 6).ALLR                                   #AT         
.* .ARETURN AGO   (&ARETN).CONVR,.SESSR,.GENR,.SECR,.FILER,.ALLR    #AT         
