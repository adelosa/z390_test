REM ZPARTRS.BAT TRACE EXECUTION SOURCE FOR %1.TRE USING LST'S AND PRN'S        
REM OUTPUT 
REM   SYSTRE -  ZPAR TRACE EXECUTION SOURCE FILE %.TRS
REM    (PSW-ADDR,CONDITION CODE,OPCODE,PRN-MODULE,PRN-ADDR,SOURCE LINE)
REM INPUT
REM   SYSTRE -  Z390 TRACE EXECUTION INSTRUCTION FILE %1.TRE
REM    (PSW-ADDR,CONDITION CODE,OPCODE,OPERAND FIELDS)
REM   SYSLST =  Z390 LINKER LIST FOR %1,LST AND AND LOAD MODULE LST'S
REM   SYSPRN =  Z390 ASSEMBLY LIST FOR %1.PRN AND ALL INCLUDED PRN'S
ERASE    ZPAR\ZPARTRS.ERR
CALL MAC ZPAR\ZPARTRS.MLC SYSPARM(%1) %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR RUNNING ZPAR\ZPARTRS.MLC
ME ZPAR\ZPARTRS.MLC ZPAR\ZPARTRS.ERR %1.TRS 
