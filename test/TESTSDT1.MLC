TESTSDT1 RT1   MAIN
         RT1   START
* TEST MZ390 SDTS
&N1      SETA  1
&N2      SETA  2
         RT1 CCL,=A(&N1),=A(&N2)   
&N1      SETA  C'B'
&N2      SETA  C'A'
         RT1 CCH,=A(&N1),=A(&N2)   
&N1      SETA  123 
&N2      SETA  123 
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  B'11'
&N2      SETA  X'3'
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  B'11000001'
&N2      SETA  C'A'
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  C' '
&N2      SETA  X'40'
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  C'ABCD'
&N2      SETA  X'C1C2C3C4'
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  B'1100000111000001'
&N2      SETA  C'AA'
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  X'100'              
&N2      SETA  256
         RT1 CCE,=A(&N1),=A(&N2)   
&N1      SETA  -1              
&N2      SETA  X'FFFFFFFF'   
         RT1 CCE,=A(&N1),=A(&N2)   
* TEST AZ390 SDTS
         RT1 CCL,=AL1(1),=AL1(2)
         RT1 CCH,=AL1(C'B'),=AL1(C'A')
         RT1 CCE,=AL1(123),=AL1(123)
         RT1 CCE,=AL1(B'11'),=AL1(X'3')
         RT1 CCE,=AL1(B'11000001'),=AL1(C'A')
         RT1 CCE,=AL1(C' '),=AL1(X'40')
         RT1 CCE,=A(C'ABCD'),=A(X'C1C2C3C4')
         RT1 CCE,=A(B'1100000111000001'),=A(C'AA')
         RT1 CCE,=A(X'100'),=A(256)
         RT1 CCE,=F'-1',=A(X'FFFFFFFF')
         RT1 END
         END         
