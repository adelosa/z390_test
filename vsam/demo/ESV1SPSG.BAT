REM BUILD VSAM ESDS CLUSTER VCDT ESV1.390 AND RUN ESV1SP, ESV1SG
CALL ASML vsam\demo\ESV1
IF ERRORLEVEL 1 PAUSE ERROR IN VCDT ESV1/390 GEN
REM RUN ESV1SP SEQ PUT TO BUILD ESV1.VES FROM 6 VLR RECORDS
SET OUTFILE=vsam\demo\ESV1.390
CALL ASMLG vsam\demo\ESV1SP %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESV1SP
REM RUN ESV1GP SEQ GET FROM ESV1.VES AND VERIFY 6 VLR RECORDS
SET INFILE=vsam\demo\ESV1.390
CALL ASMLG vsam\demo\ESV1SG %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESV1SG
