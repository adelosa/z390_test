REM BUILD VSAM ESDS CLUSTER VCDT ESF1.390 AND RUN ESF1SP, ESF1SG, REPRO
CALL ASML vsam\demo\ESF1
IF ERRORLEVEL 1 PAUSE ERROR IN VCDT ESF1/390 GEN
REM RUN ESF1SP SEQ PUT TO BUILD ESF1.VES
SET INFILE=vsam\demo\ESF1.TF1
SET OUTFILE=vsam\demo\ESF1.390
CALL ASMLG vsam\demo\ESF1SP %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESF1SP
REM RUN ESF1SG SEQ GET FROM ESF1.VES TO ESF1.TF2
SET INFILE=vsam\demo\ESF1.390
SET OUTFILE=vsam\demo\ESF1.TF2
CALL ASMLG vsam\demo\ESF1SG %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESF1SG
REM COMPARE INPUT AND OUTPUT QSAM FILES
FC vsam\demo\ESF1.TF1 vsam\demo\ESF1.TF2 > vsam\demo\ESF1SPSG.DIF
IF ERRORLEVEL 1 PAUSE ESF1SP/ESF1GP QSAM FILES DONT MATCH
NOTEPAD vsam\demo\ESF1SPSG.DIF 
