REM BUILD VSAM ESDS CLUSTER VCDT ESV1.390 AND RUN ESV1SP, ESV1SG, REPRO
CALL ASML vsam\demo\DEMOCAT
IF ERRORLEVEL 1 PAUSE ERROR IN DEMOCAT GEN
REM RUN ESV1SP SEQ PUT TO BUILD ESV1.VES
SET QFILE=vsam\demo\ESV1.TF1
SET VFILE=vsam\demo\DEMOCAT.ESV1
CALL ASMLG vsam\demo\ESV1SP %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESV1SP
REM RUN ESV1SG SEQ GET FROM ESV1.VES TO ESV1.TF2
ERASE vsam\demo\ESV1.TF2
SET VFILE=vsam\demo\DEMOCAT.ESV1
SET QFILE=vsam\demo\ESV1.TF2
CALL ASMLG vsam\demo\ESV1SG %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESV1SG
REM COMPARE INPUT AND OUTPUT QSAM FILES
FC vsam\demo\ESV1.TF1 vsam\demo\ESV1.TF2 
IF ERRORLEVEL 1 PAUSE ESV1SP/ESV1GP QSAM FILES DONT MATCH
