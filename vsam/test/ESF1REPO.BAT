REM BUILD VSAM ESDS CLUSTER VCDT ESF1.390 AND RUN REPRO LOAD/UNLOAD
CALL ASML vsam\test\TESTCAT %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN TESTCAT GEN
REM RUN REPRO TO LOAD ESF1.VES FROM ESF1.TF1
SET INFILE=vsam\test\ESF1.TF1
SET OUTFILE=vsam\test\TESTCAT.ESF1
CALL ASMLG linklib\REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO LOAD
REM SAVE REPRO LOAD STATS
COPY linklib\REPRO.LOG vsam\test\ESF1REPL.LOG  
COPY linklib\REPRO.TRE vsam\test\ESF1REPL.TRE
REM RUN REPRO TO UNLOAD FROM ESF1.VES TO ESF1.TF2
SET INFILE=vsam\test\TESTCAT.ESF1
SET OUTFILE=vsam\test\ESF1.TF2
CALL EXEC linklib\REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO UNLOAD
REM SAVE REPRO UNLOAD STATS
COPY linklib\REPRO.LOG vsam\test\ESF1REPU.LOG  
COPY linklib\REPRO.TRE vsam\test\ESF1REPU.TRE
REM COMPARE INPUT AND OUTPUT QSAM FILES
FC vsam\test\ESF1.TF1 vsam\test\ESF1.TF2 > vsam\test\ESF1REPO.DIF
IF ERRORLEVEL 1 PAUSE REPRO LOAD/UNLOAD QSAM FILES DONT MATCH

