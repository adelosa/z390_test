REM BUILD VSAM CATALOG TESTCAT AND RUN REPRO LOAD/UNLOAD OF ESV5 VT
REM RUN REPRO TO LOAD ESV5.VES FROM ESV5.TF1
SET INFILE=vsam\test\ESV5.TF1[RECFM=VT]
SET OUTFILE=vsam\test\TESTCAT.ESV5
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO LOAD
REM SAVE REPRO LOAD STATS
COPY linklib\REPRO.LOG vsam\test\ESV5REPO.RLG  
COPY linklib\REPRO.TRE vsam\test\ESV5REPO.RLE
REM RUN REPRO TO UNLOAD FROM ESV5.VES TO ESV5.TF2
SET INFILE=vsam\test\TESTCAT.ESV5
SET OUTFILE=vsam\test\ESV5.TF2[RECFM=VT]
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO UNLOAD
REM SAVE REPRO UNLOAD STATS
COPY linklib\REPRO.LOG vsam\test\ESV5REPO.RUG  
COPY linklib\REPRO.TRE vsam\test\ESV5REPO.RUE
REM COMPARE INPUT AND OUTPUT QSAM FILES
CALL COMP1 vsam\test\ESV5.TF1 vsam\test\ESV5.TF2  vsam\test\ESV5REPO.DIF
IF ERRORLEVEL 1 PAUSE REPRO LOAD/UNLOAD QSAM FILES DONT MATCH
