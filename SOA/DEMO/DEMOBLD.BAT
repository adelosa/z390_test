REM GENERATE SOA APPLICAITON
REM GEN SOA CLIENT MSG MGR
CALL ASML D:\WORK\Z390\SOA\DEMO\DEMOCMGR SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC)
IF ERRORLEVEL 1 PAUSE ERROR IN ASML DEMOCMGR
REM GEN SOA SERVER MSG MGR
CALL ASML D:\WORK\Z390\SOA\DEMO\DEMOSMGR SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC)
IF ERRORLEVEL 1 PAUSE ERROR IN ASML DEMOSMGR
REM GEN SOA SERVICE 390 FOR LOADING BY SERVER
CALL ASML D:\WORK\Z390\SOA\DEMO\DEMOSUB1 SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC) SYS390(D:\WORK\Z390\SOA\DEMO)
IF ERRORLEVEL 1 PAUSE ERROR IN ASML DEMOSUB1
REM GEN SOA SERVICE STUB OBJ FOR LINKING WITH CLIENT APPL
CALL ASM  D:\WORK\Z390\SOA\DEMO\SOA_STUB_DEMOSUB1 SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC)
IF ERRORLEVEL 1 PAUSE ERROR IN ASM STUB DEMOSUB1
COPY D:\WORK\Z390\SOA\DEMO\SOA_STUB_DEMOSUB1.OBJ D:\WORK\Z390\SOA\DEMO\DEMOSUB1.OBJ
IF ERRORLEVEL 1 PAUSE ERROR IN COPY STUB DEMOSUB1
REM GEN SOA SERVICE 390 FOR LOADING BY SERVER
CALL ASML D:\WORK\Z390\SOA\DEMO\DEMOSUB2 SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC) SYS390(D:\WORK\Z390\SOA\DEMO)
IF ERRORLEVEL 1 PAUSE ERROR IN ASML DEMOSUB2
REM GEN SOA SERVICE STUB OBJ FOR LINKING WITH CLIENT APPL
CALL ASM  D:\WORK\Z390\SOA\DEMO\SOA_STUB_DEMOSUB2 SYSMAC(D:\WORK\Z390\SOA\MACLIB+MAC)
IF ERRORLEVEL 1 PAUSE ERROR IN ASM STUB DEMOSUB2
COPY D:\WORK\Z390\SOA\DEMO\SOA_STUB_DEMOSUB2.OBJ D:\WORK\Z390\SOA\DEMO\DEMOSUB2.OBJ
IF ERRORLEVEL 1 PAUSE ERROR IN COPY STUB DEMOSUB2
REM GEN SOA MAIN CLIENT LINKED WITH SERVICE STUBS
CALL ASML D:\WORK\Z390\SOA\DEMO\DEMOMAIN OBJ390(D:\WORK\Z390\SOA\DEMO) SYS390(D:\WORK\Z390\SOA\DEMO)
