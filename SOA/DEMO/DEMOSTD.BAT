REM ASSEMBLE, STATICALLY LINK, AND EXECUTE DEMO APPLICATION LOCALLY   
CALL ASM SOA\DEMO\DEMOSUB1  %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN SOA\DEMO\DEMOSUB1
CALL ASM SOA\DEMO\DEMOSUB2  %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN SOA\DEMO\DEMOSUB2
CALL ASML SOA\DEMO\DEMOMAIN %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN SOA\DEMO\DEMOMAIN
REM RUN STATICALLY LINKED DEMOMAIN 
CALL EXEC SOA\DEMO\DEMOMAIN %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN SOA\DEMO\DEMOMAIN
