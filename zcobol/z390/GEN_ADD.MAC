               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         LE    ZC_F1,&N_SOURCE
         CGER  ZC_R3,0,ZC_F1
         AGR   ZC_R1,ZC_R3
         JNO   *+4+6
         AGI   ZC_R0,1
               AWHEN C'2'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         LD    ZC_F1,&N_SOURCE
         CGDR  ZC_R3,0,ZC_F1
         AGR   ZC_R0,ZC_R3
               AWHEN C'F',C'A',C'I'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         AGF   ZC_R1,&N_SOURCE
         JNO   *+4+6
         AGFI  ZC_R0,1
               AWHEN C'G'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         AG    ZC_R1,&N_SOURCE
         JNO   *+4+6
         AGFI  ZC_R0,1
               AWHEN C'H'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         LH    ZC_R3,&N_SOURCE
         AGFR  ZC_R1,ZC_R3
         JNO   *+4+6
         AGFI  ZC_R0,1
               AWHEN C'P'
         ZAP   ZCVT_PWORK1,&N_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
               ZC_CONV Q,LD
         ALG   ZC_R1,8+&N_TARGET
         ALCG  ZC_R0,&N_TARGET
               AWHEN C'Q'
         LMG   ZC_R0,ZC_R1,&N_TARGET
         ALG   ZC_R1,8+&N_SOURCE                    
         ALCG  ZC_R0,&N_SOURCE                     
               AWHEN C'Z'
                   ZC_PACK31  ZCVT_PWORK1,16,&N_SOURCE,&L_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
               ZC_CONV Q,LD
         ALG   ZC_R1,8+&N_TARGET
         ALCG  ZC_R0,&N_TARGET
               AELSE
                    MNOTE 8,'GEN_ADD INVALID SOURCE DATA TYPE &N_SOURCEX
               '           
                    MEXIT
               AEND 
         STMG  ZC_R0,ZC_R1,&N_TARGET
.*
.*  ADD_FLD Z    
.*
