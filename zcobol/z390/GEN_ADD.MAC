               AWHEN C'9'               ADD LB TO LD
         ZC_CONV LD,LD,&N_SOURCE
               AWHEN C'F',C'A',C'I'
         LGF   ZC_R0,&N_SOURCE
         CXGTR ZC_F0,ZC_R0
               AWHEN C'G'
         LG    ZC_R0,&N_SOURCE
         CXGTR ZC_F0,ZC_R0
               AWHEN C'H'
         LGH   ZC_R0,&N_SOURCE
         CXGTR ZC_F0,ZC_R0
