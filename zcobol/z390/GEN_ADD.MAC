         STD   ZC_F2,8+&N_TARGET
.*
.*  ADD_FLD ?? TO F/A/I
.*
         AWHEN C'F',C'A',C'I'
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'               ADD EH TO DH
         LD    ZC_F0,&N_TARGET
         LE    ZC_F1,&N_SOURCE
         LDER  ZC_F1,ZC_F1
         ADR   ZC_F0,ZC_F1
               AWHEN C'2'               ADD DH TO DH
         LD    ZC_F0,&N_TARGET
         AD    ZC_F0,&N_SOURCE
               AWHEN C'3'               ADD LH TO DH
         LD    ZC_F1,&N_TARGET
         LD    ZC_F3,8+&N_TARGET
         LDXR  ZC_F1,ZC_F1
         ADR   ZC_F0,ZC_F1
               AWHEN C'4'               ADD EB TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,EB,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'5'               ADD DB TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,DB,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'6'               ADD LB TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,LB,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'7'               ADD ED TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,ED,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'8'               ADD DD TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,DD,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'9'               ADD LB TO DH
         LD    ZC_F1,&N_TARGET
         ZC_CONV DH,LD,&N_SOURCE
         ADR   ZC_F0,ZC_F1
               AWHEN C'2'
         L     ZC_R0,&N_TARGET
         LD    ZC_F1,&N_SOURCE
         CFDR  ZC_R3,0,ZC_F1
         AR    ZC_R0,ZC_R3
         ST    ZC_R0,&N_TARGET
               AWHEN C'1'
         L     ZC_R0,&N_TARGET
         LE    ZC_F1,&N_SOURCE
         CFER  ZC_R3,0,ZC_F1
         AR    ZC_R0,ZC_R3
         ST    ZC_R0,&N_TARGET
               AWHEN C'F',C'A',C'I'
         L     ZC_R0,&N_TARGET
         A     ZC_R0,&N_SOURCE
         ST    ZC_R0,&N_TARGET
               AWHEN C'G'
         L     ZC_R0,&N_TARGET
         A     ZC_R0,4+&N_SOURCE IGNORE HIGH 32 BITS
         ST    ZC_R0,&N_TARGET  
               AWHEN C'H'
         L     ZC_R0,&N_TARGET
         AH    ZC_R0,&N_SOURCE
         ST    ZC_R0,&N_TARGET
               AWHEN C'P'
         ZAP   ZCVT_PWORK1,&N_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
         ZC_CONV Q,LD
         A     ZC_R1,&N_TARGET IGNORE HIGH 96 BITS
         ST    ZC_R1,&N_TARGET
               AWHEN C'Q'
         L     ZC_R0,&N_TARGET
         A     ZC_R0,12+&N_SOURCE IGNORE HIGH 48 BITS
         ST    ZC_R0,&N_TARGET  
               AWHEN C'Z'
                   ZC_PACK31  ZCVT_PWORK1,16,&N_SOURCE,&L_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
                   ZC_CONV Q,LD
         A     ZC_R1,&N_TARGET IGNORE HIGH 96 BITS
         ST    ZC_R1,&N_TARGET 
               AELSE
                    MNOTE 8,'GEN_ADD INVALID SOURCE DATA TYPE &N_SOURCEX
               '           
                    MEXIT
               AEND 
.*
.*  ADD_FLD ?? TO G    
.*
         AWHEN C'G'
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'2'
         LG    ZC_R0,&N_TARGET
         LD    ZC_F1,&N_SOURCE
         CGDR  ZC_R3,0,ZC_F1
         AGR   ZC_R0,ZC_R3
         STG   ZC_R0,&N_TARGET
               AWHEN C'1'
         LG    ZC_R0,&N_TARGET
         LE    ZC_F1,&N_SOURCE
         CGER  ZC_R3,0,ZC_F1
         AGR   ZC_R0,ZC_R3
         STG   ZC_R0,&N_TARGET
               AWHEN C'F',C'A',C'I'
         LG    ZC_R0,&N_TARGET
         AGF   ZC_R0,&N_SOURCE
         STG   ZC_R0,&N_TARGET
               AWHEN C'G'
         LG    ZC_R0,&N_TARGET
         AG    ZC_R0,&N_SOURCE
         STG   ZC_R0,&N_TARGET                          
               AWHEN C'H'
         LG    ZC_R0,&N_TARGET
         LH    ZC_R3,&N_SOURCE
         AGFR  ZC_R0,ZC_R3
         STG   ZC_R0,&N_TARGET
               AWHEN C'P'
         ZAP   ZCVT_PWORK1,&N_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
         ZC_CONV Q,LD
         AG    ZC_R1,&N_TARGET
         STG   ZC_R1,&N_TARGET
               AWHEN C'Q'
         LG    ZC_R0,&N_TARGET
         AG    ZC_R0,8+&N_SOURCE IGNORE HIGH 64 BITS
         STG   ZC_R0,&N_TARGET  
               AWHEN C'Z'
                   ZC_PACK31  ZCVT_PWORK1,16,&N_SOURCE,&L_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
                   ZC_CONV Q,LD
         AG    ZC_R1,&N_TARGET  IGNORE HIGH 64
         STG   ZC_R1,&N_TARGET
               AELSE
                    MNOTE 8,'GEN_ADD INVALID SOURCE DATA TYPE &N_SOURCEX
               '           
                    MEXIT
               AEND 
.*
.*  ADD_FLD ?? TO H    
.*
         AWHEN C'H'
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'2'
         LH    ZC_R0,&N_TARGET
         LD    ZC_F3,&N_SOURCE
         CFDR  ZC_R1,0,ZC_F3
         AR    ZC_R0,ZC_R1
         STH   ZC_R0,&N_TARGET
               AWHEN C'1'
         LH    ZC_R0,&N_TARGET
         LE    ZC_F1,&N_SOURCE
         CFER  ZC_R3,0,ZC_F1
         AR    ZC_R0,ZC_R3
         STH   ZC_R0,&N_TARGET
               AWHEN C'F',C'A',C'I'
         LH    ZC_R0,&N_TARGET
         A     ZC_R0,&N_SOURCE
         STH   ZC_R0,&N_TARGET
               AWHEN C'G'
         LH    ZC_R0,&N_TARGET
         A     ZC_R0,&N_SOURCE+4 IGNORES HIGH 32
         STH   ZC_R0,&N_TARGET
               AWHEN C'H'
         LH    ZC_R0,&N_TARGET
         AH    ZC_R0,&N_SOURCE
         STH   ZC_R0,&N_TARGET
               AWHEN C'P'
         ZAP   ZCVT_PWORK1,&N_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
         ZC_CONV Q,LD
         AH    ZC_R1,&N_TARGET  IGNORE HIGH 96 BITS
         STH   ZC_R1,&N_TARGET
               AWHEN C'Z'
                   ZC_PACK31  ZCVT_PWORK1,16,&N_SOURCE,&L_SOURCE
         LMG   ZC_R0,ZC_R1,ZCVT_PWORK1
         CXSTR ZC_F4,ZC_R0
                   ZC_CONV Q,LD
         AH    ZC_R1,&N_TARGET  IGNORE HIGH 96
         STH   ZC_R1,&N_TARGET  IGNORE HIGH 112
               AELSE
                    MNOTE 8,'GEN_ADD INVALID SOURCE DATA TYPE &N_SOURCEX
               '           
                    MEXIT
               AEND 
.*
.*  ADD_FLD ?? TO P    
.*
         AWHEN C'P'
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'
         LE    ZC_F0,&N_SOURCE
         CFER  ZC_R0,0,ZC_F0
         CVD   ZC_R0,ZCVT_PWORK
         AP    &N_TARGET,ZCVT_PWORK
               AWHEN C'2'
         LD    ZC_F0,&N_SOURCE
         CFDR  ZC_R0,0,ZC_F0
         CVD   ZC_R0,ZCVT_PWORK
