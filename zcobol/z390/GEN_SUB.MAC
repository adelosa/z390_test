         GEN_SUB &T_NAME,&T_IX,&S_NAME,&S_IX,&ROUNDED,&ONSIZE 
         COPY  ZC_WS
         AIF (&T_IX GT 0)
             :&N_TARGET SETC '&T_NAME'
             :&L_TARGET SETA  &SYM_LEN(&T_IX)
             :&S_TARGET SETB  &SYM_PIC_SIGN(&T_IX)
             :&T_TARGET SETC '&SYM_PIC_TYPE(&T_IX)'
             :&P_TARGET SETC '&SYM_PIC(&T_IX)'
         AELSEIF (&T_IX LT 0)
             :&N_TARGET SETC '&TMP_NAME(-&T_IX)'
             :&L_TARGET SETA  &TMP_LEN(-&T_IX)
             :&S_TARGET SETB  &TMP_SIGN(-&T_IX)
             :&T_TARGET SETC '&TMP_TYPE(-&T_IX)'
             :&P_TARGET SETC ''
             AIF ('&T_TARGET' EQ 'P')
                 :&N_TARGET SETC '&N_TARGET.(&L_TARGET)'
             AEND
         AELSE
            MNOTE 8,'GEN_SUB MISSING TARGET INDEX FOR &T_NAME'
            MEXIT
         AEND
         :&N_SOURCE SETC '&S_NAME'
         AIF (&S_IX GT 0)
             :&L_SOURCE SETA  &SYM_LEN(&S_IX)
             :&S_SOURCE SETB  &SYM_PIC_SIGN(&S_IX)
             :&T_SOURCE SETC '&SYM_PIC_TYPE(&S_IX)'
         AELSEIF (&S_IX LT 0)
             :&N_SOURCE SETC '&TMP_NAME(-&S_IX)'
             :&L_SOURCE SETA  &TMP_LEN(-&S_IX)
             :&S_SOURCE SETB  &TMP_SIGN(-&S_IX)
             :&T_SOURCE SETC '&TMP_TYPE(-&S_IX)'
             AIF ('&T_SOURCE' EQ 'P')
                 :&N_SOURCE SETC '&N_SOURCE.(&L_SOURCE)'
             AEND
         AEND
         AIF   (&ROUNDED EQ 1)
               MNOTE 8,'GEN_SUB ROUNDED NOT SUPPORTED YET'
               MEXIT
         AEND
         AIF   (&ONSIZE EQ 1)
               MNOTE 8,'GEN_SUB ON SIZE ERROR NOT SUPPORTED YET'
               MEXIT
         AEND
         AIF   (&S_IX NE 0)
               ACALL SUB_FLD
         AELSE
               :&LIT SETC '&N_SOURCE'
               ACALL SUB_LIT
         AEND
         MEXIT
.*
.* SUBTRACT LITERAL
.* 
         AENTRY SUB_LIT
         AIF  ('&LIT'(1,1) GE '0'                                      X
                OR '&LIT'(1,1) EQ '+'                                  X
                OR '&LIT'(1,1) EQ '-') RPI 1002 WAS GT '0'
               :&NUM SETC '&LIT'
               ACALL SUB_NUM_LIT
         AELSEIF  ('&LIT' EQ 'ZERO'                                    X
                   OR '&LIT' EQ 'ZEROES')                          
               :&NUM SETC '0'
               ACALL SUB_NUM_LIT
         AELSE
               MNOTE 8,'GEN_SUB UNKNOWN LITERAL &LIT'
               MEXIT
         AEND
         AEND
.*
.* SUB NUMERIC LIT 
.*
      AENTRY SUB_NUM_LIT
      ACASE (C2A('&T_TARGET'))
      AWHEN C'1'
         LE    F0,&N_TARGET
         LARL  R14,=EH'&NUM'
         SE    F0,0(R14)
         STE   F0,&N_TARGET
      AWHEN C'2'
         LD    F0,&N_TARGET
         LARL  R14,=DH'&NUM'
         SD    F0,0(R14)
         STD   F0,&N_TARGET
      AWHEN C'3'
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LARL  R14,=LH'&NUM'
         LD    F1,0(R14)
         LD    F3,8(R14)
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
      AWHEN C'4'
         LE    F0,&N_TARGET
         LARL  R14,=EB'&NUM'
         SEB   F0,0(R14)
         STE   F0,&N_TARGET
      AWHEN C'5'
         LD    F0,&N_TARGET
         LARL  R14,=DB'&NUM'
         SDB   F0,0(R14)
         STD   F0,&N_TARGET
      AWHEN C'6'
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LARL  R14,=LB'&NUM'
         LD    F1,0(R14)
         LD    F3,8(R14)
         SXBR  F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
      AWHEN C'7'
         LE    F0,&N_TARGET
         LDETR F0,F0,0
         LARL  R14,=DD'&NUM'
         LD    F1,0(R14)
         SDTR  F0,F0,F1
         LEDTR F0,0,F0,0
         STE   F0,&N_TARGET
      AWHEN C'8'
         LD    F0,&N_TARGET
         LARL  R14,=DD'&NUM'
         LD    F1,0(R14)
         SDTR  F0,F0,F1
         STD   F0,&N_TARGET
      AWHEN C'9'
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LARL  R14,=LD'&NUM'
         LD    F1,0(R14)
         LD    F3,8(R14)
         SXTR  F0,F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
      AWHEN C'F',C'A',C'I'
         L     R0,&N_TARGET
         AFI   R0,-&NUM
         ST    R0,&N_TARGET
      AWHEN C'H'
         LH    R0,&N_TARGET
         AHI   R0,-&NUM
         STH   R0,&N_TARGET
      AWHEN C'G'
         LG    R0,&N_TARGET
         LARL  R14,=FL8'&NUM'
         SG    R0,0(R14)
         STG   R0,&N_TARGET
      AWHEN C'P'
         :&LPNUM SETA (K'&NUM/2+3)/2*2 ROUNDED EVEN FOR LARL
         AIF (&LPNUM GT 16)
             :&LPNUM SETA 16
         AEND
         LARL  R14,=PL&LPNUM'&NUM'
         SP    &N_TARGET,0(&LPNUM,R14)
      AWHEN C'Q'
         LMG   R0,R1,&N_TARGET
         LARL  R14,=FL16'&NUM'
         SLG   R1,8(R14)    
         SLBG  R0,0(R14)                      
         STMG  R0,R1,&N_TARGET
      AWHEN C'Z'
         :&PLEN1 SETA &L_TARGET/2+1
         ZC_PACK31 ZCVT_PWORK1,&PLEN1,&N_TARGET,&L_TARGET
         :&LPNUM SETA (K'&NUM/2+3)/2*2 ROUNDED EVEN FOR LARL
         AIF (&LPNUM GT 16)
             :&LPNUM SETA 16
         AEND
         LARL  R14,=PL&LPNUM'&NUM'
         SP    ZCVT_PWORK1(&PLEN1),0(&LPNUM,R14)
         :&ST SETB &S_TARGET
         ZC_UNPK31 &N_TARGET,&L_TARGET,&ST,ZCVT_PWORK1,&PLEN1
      AELSE
         MNOTE 8,'GEN_SUB INVALID DATA TYPE FOR &N_TARGET'
      AEND
      AEND
.*
.* SUB_FLD ?? FROM FLD ??
.*
         AENTRY SUB_FLD
         ACASE (C2A('&T_TARGET'))
         AWHEN C'1'                      SUBTRACT ?? FROM EH
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'                SUBTRACT EH FROM EH
         LE    F0,&N_TARGET           
         LE    F1,&N_SOURCE
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'2'                SUBTRACT DH FROM EH
         LE    F0,&N_TARGET
         LD    F1,&N_SOURCE
         LEDR  F1,F1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'3'                SUBTRACT LH FROM EH
         LE    F0,&N_TARGET
         LD    F1,&N_SOURCE
         LD    F3,&N_SOURCE
         LEXR  F1,F1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'4'                SUBTRACT EB FROM EH
                 ZC_CONV EH,EB,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'5'                SUBTRACT DB FROM EH
                 ZC_CONV EH,DB,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'6'                SUBTRACT LB FROM EH
                 ZC_CONV EH,LB,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'7'                SUBTRACT ED FROM EH
                 ZC_CONV EH,ED,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'8'                SUBTRACT DD FROM EH
                 ZC_CONV EH,DD,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'9'                SUBTRACT LD FROM EH
                 ZC_CONV EH,LD,&N_SOURCE
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'F',C'A',C'I'
         LE    F0,&N_TARGET
         L     R1,&N_SOURCE
         CEFR  F1,R1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'G'
         LE    F0,&N_TARGET
         LG    R1,&N_SOURCE
         CEGR  F1,R1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'H'
         LE    F0,&N_TARGET
         LH    R1,&N_SOURCE
         CEFR  F1,R1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'P'
         LE    F0,&N_TARGET
         ZAP   ZCVT_PWORK,&N_SOURCE
         CVB   R1,ZCVT_PWORK
         CEFR  F1,R1
         SER   F0,F1
         STE   F0,&N_TARGET
               AWHEN C'Q'
                 ZC_CONV EH,Q,&N_SOURCE         
         LE    F1,&N_TARGET
         SER   F1,F0
         STE   F1,&N_TARGET
               AWHEN C'Z'
         LE    F0,&N_TARGET
               ZC_PACK31 ZCVT_PWORK2,16,&N_SOURCE,&L_SOURCE
         CVBG  R1,ZCVT_PWORK2
         CEFR  F1,R1
         SER   F0,F1
         STE   F0,&N_TARGET
               AELSE
                    MNOTE 8,'GEN_SUB INVALID SOURCE TYPE &N_SOURCE'           
                    MEXIT
               AEND 
         AWHEN C'2'                      SUBTRACT ?? FROM DH
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'                SUBTRACT EH FROM DH  
         LD    F0,&N_TARGET                                  
         LDE   F1,&N_SOURCE
         SDR   F0,F1
         STD   F0,&N_TARGET
               AWHEN C'2'                SUBTRACT DH FROM DH
         LD    F0,&N_TARGET
         SD    F0,&N_SOURCE
         STD   F0,&N_TARGET
               AWHEN C'3'                SUBTRACT LH FROM DH
         LD    F0,&N_TARGET
         LD    F1,&N_SOURCE
         LD    F3,8+&N_SOURCE
         LDXR  F1,F1
         SDR   F0,F1
         STD   F0,&N_TARGET
               AWHEN C'4'                SUBTRACT EB FROM DH  
                 ZC_CONV DH,EB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'5'                SUBTRACT DB FROM DH
                 ZC_CONV DH,DB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'6'                SUBTRACT LB FROM DH
                 ZC_CONV DH,LB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'7'                SUBTRACT ED FROM DH  
                 ZC_CONV DH,ED,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'8'                SUBTRACT DD FROM DH
                 ZC_CONV DH,DD,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'9'                SUBTRACT LD FROM DH
                 ZC_CONV DH,LD,&N_SOURCE
         LD    F1,&N_TARGET           
         SDR   F1,F0
         STD   F1,&N_TARGET
               AWHEN C'F',C'A',C'I'
         LD    F0,&N_TARGET
         L     R1,&N_SOURCE
         CDFR  F1,R1
         SDR   F0,F1
         STD   F0,&N_TARGET
               AWHEN C'G'
         LD    F0,&N_TARGET
         LG    R1,&N_SOURCE
         CDGR  F1,R1
         SDR   F0,F1
         STD   F0,&N_TARGET
               AWHEN C'H'
         LD    F0,&N_TARGET
         LH    R1,&N_SOURCE
         CDFR  F1,R1
         SDR   F0,F1
         STD   F0,&N_TARGET
               AWHEN C'P'
         LD    F0,&N_TARGET
         ZAP   ZCVT_PWORK,&N_SOURCE
         CVB   R1,ZCVT_PWORK
         CDFR  F1,R1
         SDR   F0,R1
         STD   F0,&N_TARGET
               AWHEN C'Q'
         LMG   R0,R1,&N_SOURCE
         ZC_CONV DH,Q         
         SD    F0,&N_TARGET
         STD   F0,&N_TARGET
               AWHEN C'Z'
         LD    R0,&N_TARGET
         ZC_PACK31 ZCVT_PWORK2,16,&N_SOURCE,&L_SOURCE
         CVBG  R1,ZCVT_PWORK2
         CDFR  F1,R1
         SDR   R0,F1
         STD   F0,&N_TARGET
               AELSE
                    MNOTE 8,'GEN_SUB INVALID SOURCE TYPE &N_SOURCE'           
                    MEXIT
               AEND 
         AWHEN C'3'                      SUBTRACT ?? FROM LH
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'                SUBTRACT EH FROM LH     
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LE    F1,&N_SOURCE
         LXER  F1,F1
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'2'                SUBTRACT DH FROM LH     
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LD    F1,&N_SOURCE
         LXDR  F1,F1
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'3'                SUBTRACT LH FROM LH     
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LD    F1,&N_SOURCE
         LD    F3,8+&N_SOURCE
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'4'                SUBTRACT EB FROM LH   
                 ZC_CONV LH,EB,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'5'                SUBTRACT DB FROM LH     
                 ZC_CONV LH,DB,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'6'                SUBTRACT LB FROM LH     
                 ZC_CONV LH,LB,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'7'                SUBTRACT ED FROM LH   
                 ZC_CONV LH,ED,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'8'                SUBTRACT DD FROM LH     
                 ZC_CONV LH,DD,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'9'                SUBTRACT LD FROM LH     
                 ZC_CONV LH,LD,&N_SOURCE  
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'F',C'A',C'I'      SUBTRACT F  FROM LH
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         L     R1,&N_SOURCE
         CXFR  F1,R1
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'G'               SUBTRACT G  FROM LH
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LG    R1,&N_SOURCE
         CXGR  F1,R1
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'H'              SUBTRACT H  FROM LH
         LD    F0,&N_TARGET
         LD    F2,8+&N_TARGET
         LH    R1,&N_SOURCE
         CXFR  F1,R1
         SXR   F0,F1
         STD   F0,&N_TARGET
         STD   F2,8+&N_TARGET
               AWHEN C'P'              SUBTRACT P  FROM LH
         ZAP   ZCVT_PWORK2,&N_SOURCE
         LMG   R0,R1,ZCVT_PWORK2
         CXSTR F4,R0
                 ZC_CONV LH,LD
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'Q'              SUBTRACT Q  FROM LH
         LMG   R0,R1,&N_SOURCE
         ZC_CONV LH,Q         
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0               
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AWHEN C'Z'              SUBTRACT Z  FROM LH
         ZC_PACK31 ZCVT_PWORK2,16,&N_SOURCE,&L_SOURCE
         LMG   R0,R1,ZCVT_PWORK2
         CXSTR F4,R0
                 ZC_CONV LH,LD
         LD    F1,&N_TARGET
         LD    F3,8+&N_TARGET
         SXR   F1,F0               
         STD   F1,&N_TARGET
         STD   F3,8+&N_TARGET
               AELSE
                    MNOTE 8,'GEN_SUB INVALID SOURCE TYPE &N_SOURCE'           
                    MEXIT
               AEND 
         AWHEN C'4'                      SUBTRACT ?? FROM EB
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'                SUBTRACT EH FROM EB
                 ZC_CONV EB,EH,&N_SOURCE
         LE    F1,&N_TARGET           
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'2'                SUBTRACT DH FROM EB
                 ZC_CONV EB,DH,&N_SOURCE
         LE    F1,&N_TARGET           
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'3'                SUBTRACT LH FROM EB
                 ZC_CONV EB,LH,&N_SOURCE
         LE    F1,&N_TARGET           
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'4'                SUBTRACT EB FROM EB
                 ZC_CONV EB,EB,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'5'                SUBTRACT DB FROM EB
                 ZC_CONV EB,DB,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'6'                SUBTRACT LB FROM EB
                 ZC_CONV EB,LB,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'7'                SUBTRACT ED FROM EB
                 ZC_CONV EB,ED,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'8'                SUBTRACT DD FROM EB
                 ZC_CONV EB,DD,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'9'                SUBTRACT LD FROM EB
                 ZC_CONV EB,LD,&N_SOURCE
         LE    F1,&N_TARGET
         SEBR  F1,F0
         STE   F1,&N_TARGET
               AWHEN C'F',C'A',C'I'      SUBTRACT F  FROM EB
         LE    F0,&N_TARGET
         L     R1,&N_SOURCE
         CEFBR F1,R1
         SEBR  F0,F1
         STE   F0,&N_TARGET
               AWHEN C'G'                SUBTRACT G  FROM EB
         LE    F0,&N_TARGET
         LG    R1,&N_SOURCE
         CEGBR F1,R1
         SEBR  F0,F1
         STE   F0,&N_TARGET
               AWHEN C'H'                SUBTRACT H  FROM EB
         LE    F0,&N_TARGET
         LH    R1,&N_SOURCE
         CEFBR F1,R1
         SEBR  F0,F1
         STE   F0,&N_TARGET
               AWHEN C'P'                SUBTRACT P  FROM EB
         LE    F0,&N_TARGET
         ZAP   ZCVT_PWORK,&N_SOURCE
         CVB   R1,ZCVT_PWORK
         CEFBR F1,R1
         SEBR  F0,F1
         STE   F0,&N_TARGET
               AWHEN C'Q'               SUBTRACT Q  FROM EB
         LMG   R0,R1,&N_SOURCE
         ZC_CONV EB,Q         
         LE    F1,&N_TARGET
         SEBR  F1,F0                   
         STE   F1,&N_TARGET
               AWHEN C'Z'               SUBTRACT Z  FROM EB
         LE    F0,&N_TARGET
               ZC_PACK31 ZCVT_PWORK2,16,&N_SOURCE,&L_SOURCE
         LMG   R0,R1,ZCVT_PWORK2
         CXSTR F4,R0
               ZC_CONV EB,LH
         LE    F1,&N_TARGET
         SEBR  F1,F0                   
         STE   F1,&N_TARGET
               AELSE
                    MNOTE 8,'GEN_SUB INVALID SOURCE TYPE &N_SOURCE'           
                    MEXIT
               AEND 
         AWHEN C'5'                      SUBTRACT ?? FROM DB
               ACASE (C2A('&T_SOURCE'))
               AWHEN C'1'                SUBTRACT EH FROM DB  
                 ZC_CONV DB,EH,&N_SOURCE
         LD    F1,&N_TARGET                                  
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'2'                SUBTRACT DH FROM DB
                 ZC_CONV DB,DH,&N_SOURCE
         LD    F1,&N_TARGET                                  
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'3'                SUBTRACT LH FROM DB
                 ZC_CONV DB,LH,&N_SOURCE
         LD    F1,&N_TARGET                                  
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'4'                SUBTRACT EB FROM DB  
                 ZC_CONV DB,EB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'5'                SUBTRACT DB FROM DB
                 ZC_CONV DB,DB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'6'                SUBTRACT LB FROM DB
                 ZC_CONV DB,LB,&N_SOURCE
         LD    F1,&N_TARGET           
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'7'                SUBTRACT ED FROM DB  
                 ZC_CONV DB,ED,&N_SOURCE
         LD    F1,&N_TARGET           
         SDBR  F1,F0
         STD   F1,&N_TARGET
               AWHEN C'8'                SUBTRACT DD FROM DB
